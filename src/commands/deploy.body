\n.pwat-card__info .pwat-card__full-title {\r\n    font-weight: 600;\r\n    font-size: 17px;\r\n}\r\n\r\n@media (max-width: 1024px) {\r\n    .pwat-card__product,\r\n    .pwat-card__product img {\r\n        width: 100%;\r\n    }\r\n\r\n    .pwat-card__button {\r\n        width: 100%;\r\n        font-size: 10px;\r\n        height: 40px;\r\n        bottom: 10px;\r\n    }\r\n\r\n    .pwat-card__button img {\r\n        width: 15px;\r\n    }\r\n\r\n    .pwat-card__button img {\r\n    width: 15px;\r\n    }\r\n\r\n    .pwat-card__product .pwat-info__icon {\r\n        bottom: -5px;\r\n        position: absolute;\r\n        right: 15px;\r\n    }\r\n\r\n    .pwat-card__product .pwat-card__button {\r\n        opacity: 1;\r\n        visibility: visible;\r\n        transition: all .5s;\r\n    }\r\n}"},{"metadata":"css","name":"__css_product-page","extension":"css","content":"/**\r\n* PÁGINA DE PRODUTO\r\n\r\n* class .pwat-breadcumb-*\r\n*/\r\n.pwat-breadcumb {\r\n  margin-top: 40px;\r\n  display: flex;\r\n  flex-flow: row wrap;\r\n  justify-content: flex-start;\r\n  align-items: center;\r\n}\r\n\r\n.pwat-breadcumb .pwat-breadcumb__item {\r\n  font-size: 14px;\r\n  color: #2f2f2f;\r\n}\r\n\r\n.pwat-breadcumb .pwat-breadcumb__item:after {\r\n  content: '>';\r\n  margin-left: 10px;\r\n  margin-right: 10px;\r\n}\r\n\r\n.pwat-breadcumb .pwat-breadcumb__item:last-child:after {\r\n  content: '';\r\n}\r\n\r\n/**\r\n* class .pwat-gallery-*\r\n*/\r\n.pwat-gallery {\r\n  margin-top: 50px;\r\n  display: flex;\r\n  flex-flow: row;\r\n  justify-content: flex-start;\r\n  align-items: flex-start;\r\n  padding: 50px;\r\n  border: 1px solid #dcdcdc;\r\n  border-radius: 10px;\r\n}\r\n.pwat-gallery__main-image {\r\n    position: relative;\r\n    width: 549px;\r\n}\r\n\r\n.pwat-gallery__view-zoom {\r\n    position: absolute;\r\n    bottom: 20px;\r\n    right: 20px;\r\n    z-index: 99;\r\n    width: 45px;\r\n    height: 45px;\r\n    background: #fff;\r\n    display: flex;\r\n    border-radius: 50%;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n.pwat-gallery__view-zoom img {\r\n    width: 20px;\r\n    height: 20px;\r\n}\r\n.zoomContainer {\r\n    z-index: 9;\r\n}\r\n\r\n.pwat-gallery__top {\r\n    width: 100%;\r\n}\r\n.pwat-gallery__list-images {\r\n  display: flex;\r\n  flex-flow: row wrap;\r\n  justify-content: flex-start;\r\n  align-items: center;\r\n  margin-top: 10px;\r\n  width: 100%;\r\n}\r\n.pwat-gallery__list-images a:not(:last-child) {\r\n  margin-right: 10px;\r\n}\r\n.pwat-gallery__imagens {\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    width: 50%;\r\n}\r\n.pwat-gallery__list-images img {\r\n  width: 80px;\r\n  margin-right: 10px;\r\n  cursor: pointer;\r\n}\r\n.pwat-gallery__list-images img:last-child {\r\n  margin-right: 0;\r\n}\r\n.pwat-gallery__info {\r\n    margin-left: 100px;\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    width: 100%;\r\n    padding: 20px;\r\n    background: #f8f8f8;\r\n    border-radius: 10px;\r\n    box-shadow: 0 0 40px -10px #d5d5d5;\r\n    z-index: 2;\r\n    position: relative;\r\n}\r\n.pwat-gallery__info .pwat-card__favorite-container {\r\n    top: 10px !important;\r\n    right: 10px !important;\r\n}\r\n.pwat-gallery__info-title {\r\n  font-size: 22px;\r\n  font-weight: 500;\r\n  margin-bottom: 10px;\r\n  color: #2f2f2f;\r\n}\r\n.pwat-gallery__info-sku {\r\n  font-size: 12px;\r\n  font-weight: 500;\r\n  margin-bottom: 20px;\r\n}\r\n.pwat-gallery__info-submit {\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    margin-bottom: 40px;\r\n    width: 100%;\r\n}\r\n.pwat-gallery__share {\r\n  margin-bottom: 40px;\r\n}\r\n.pwat-gallery__share a:last-child {\r\n  margin-right: 0;\r\n}\r\n.pwat-gallery__share a {\r\n  margin-right: 15px;\r\n}\r\n.pwat-gallery__share a i {\r\n  font-size: 18px;\r\n}\r\n.pwat-gallery__description {\r\n  display: flex;\r\n  flex-flow: column;\r\n  justify-content: flex-start;\r\n  align-items: flex-start;\r\n}\r\n.pwat-gallery__description h3 {\r\n  font-size: 13px;\r\n  font-weight: 600;\r\n  text-transform: uppercase;\r\n  margin-bottom: 10px;\r\n}\r\n.pwat-input__number {\r\n  width: 160px;\r\n  height: 40px;\r\n  display: flex;\r\n  flex-flow: row;\r\n  justify-content: flex-start;\r\n  align-items: flex-start;\r\n  margin-bottom: 20px;\r\n}\r\n.pwat-input__number button {\r\n  width: 40px;\r\n  height: 42px;\r\n  background: #fff;\r\n  border: 1px solid #dcdcdc;\r\n  font-size: 25px;\r\n  color: #01151e;\r\n}\r\n.pwat-input__number input {\r\n  width: 78px;\r\n  height: 40px;\r\n  border: none;\r\n  border-top: 1px solid #dcdcdc;\r\n  border-bottom: 1px solid #dcdcdc;\r\n  text-align: center;\r\n}\r\n.pwat-gallery__info-price {\r\n    margin-bottom: 20px;\r\n}\r\n.pwat-gallery__info-price .discount {\r\n    font-size: 12px;\r\n    text-decoration: line-through;\r\n}\r\n.pwat-gallery__submit-button {\r\n    width: 100%;\r\n    height: 50px;\r\n    background: #01151e;\r\n    color: #fff;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    font-size: 14px;\r\n    transition: all .5s;\r\n    border-radius: 30px;\r\n}\r\n.pwat-gallery__submit-button:hover {\r\n    background: #ebebeb;\r\n    color: #01151e;\r\n    transition: all .5s;\r\n}\r\n.gallery-top {\r\n    height: 80%;\r\n    width: 100%;\r\n}\r\n.gallery-thumbs {\r\n    height: 20%;\r\n    box-sizing: border-box;\r\n    padding: 10px 0;\r\n}\r\n.gallery-thumbs .swiper-slide {\r\n    width: 25% !important;\r\n    height: 100%;\r\n    opacity: 0.4;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    margin-top: 20px;\r\n    margin-bottom: 20px;\r\n    border-radius: 5px;\r\n    border: 1px solid #ededed;\r\n}\r\n.gallery-thumbs .swiper-slide-thumb-active {\r\n    opacity: 1;\r\n}\r\n.pwat-input__number button:last-child {\r\n    border-top-right-radius: 20px;\r\n    border-bottom-right-radius: 20px;\r\n}\r\n.pwat-input__number button:first-child {\r\n    border-top-left-radius: 20px;\r\n    border-bottom-left-radius: 20px;\r\n}\r\n\r\n\r\n/**\r\n* class .pwat-product__details-*\r\n*/\r\n.pwat-product__details {\r\n  padding: 50px;\r\n  border: 1px solid #dcdcdc;\r\n  margin-top: 40px;\r\n  border-radius: 10px;\r\n}\r\n\r\n.pwat-product__details-table {\r\n  border-collapse: collapse;\r\n  width: 100%;\r\n}\r\n\r\n.pwat-product__details-table tr:nth-child(even) {\r\n  background-color: #dcdcdc;\r\n}\r\n\r\n.pwat-product__details-table td, \r\n.pwat-product__details-table th {\r\n    text-align: left;\r\n    padding: 8px;\r\n}\r\n\r\n.pwat-product__details-table td { width: 50%; }\r\n\r\n\r\n/**\r\n    Product configurations & variations\r\n*/\r\n.pwat-gallery__configurations {\r\n    margin-bottom: 20px;\r\n}\r\n.p4a-conf-bundle__option-color--list .p4a-conf-bundle__option-color--value {\r\n    display: none;\r\n}\r\n.p4a-conf-bundle__option-color label input {\r\n    position: absolute;\r\n    visibility: hidden;\r\n    opacity: 0;\r\n}\r\n.p4a-conf-bundle__option-color .p4a-conf-bundle__option-color--title {\r\n    font-size: 0;\r\n}\r\n.p4a-conf-bundle__option-color--list .p4a-conf-bundle__option-color--title {\r\n    font-size: 17px !important;\r\n    margin: 0;\r\n}\r\n.p4a-conf-bundle__option-color label[data-checked=\"true\"] {\r\n    border: 1px solid #010101;\r\n    border-radius: 5px;\r\n    width: 35px;\r\n    height: 35px;\r\n    padding: 0;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n.p4a-conf-bundle__option-color--value {\r\n    border-radius: 5px;\r\n    border: 1px solid #ededed;\r\n}\r\n\r\n\r\n/**\r\n* class .pwat-warning__addcart\r\n*/\r\n.pwat-warning__addcart {\r\n  position: fixed;\r\n  bottom: 50px;\r\n  left: 0;\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  visibility: hidden;\r\n  opacity: 0;\r\n  transition: all .5s;\r\n}\r\n.pwat-warning__addcart--active {\r\n  visibility: visible !important;\r\n  opacity: 1 !important;\r\n  transition: all .5s;\r\n}\r\n.pwat-warning__addcart span {\r\n  background: #01151e;\r\n  height: 45px;\r\n  padding: 0 20px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  border-radius: 20px;\r\n}\r\n\r\n\r\n/* LINKED PRODUCTS */\r\n.pwat-product__linkeds {\r\n    margin-top: 40px;\r\n}\r\n.pwat-product__linkeds .pwat-product__details {\r\n    margin-top: 15px;\r\n}\r\n.pwat-product__linkeds h2 {\r\n    font-weight: 500;\r\n    color: #01151e;\r\n    font-size: 25px;\r\n}\r\n.pwat-product__linkeds-relateds-container {\r\n    padding-bottom: 30px !important;\r\n}\r\n.pwat-product__linkeds .swiper-pagination {\r\n    bottom: 0px !important;\r\n}\r\n\r\n\r\n/* PRODUCT KIT */\r\n.pwat-product__kits {\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    margin-top: 40px;\r\n}\r\n.pwat-product__details-kit {\r\n    display: flex;\r\n    flex-flow: row wrap;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    width: 100%;\r\n}\r\n.pwat-product__details-kit-product {\r\n    width: calc(25% - 20px);\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n    box-shadow: 0 0 15px -5px #ccccccb8;\r\n    padding: 10px;\r\n}\r\n.pwat-product__details-kit-product:not(:last-child) {\r\n    margin-right: 10px;\r\n    width: calc(25% - 30px);\r\n}\r\n.pwat-product__details-kit-product strong {\r\n    font-size: 12px;\r\n    font-weight: 500;\r\n    text-align: center;\r\n}\r\n.pwat-product__details-kit-product img {\r\n    width: 100%;\r\n}\r\n.pwat-product__kits .pwat-product__details {\r\n    margin-top: 15px;\r\n}\r\n.pwat-product__kits h2 {\r\n    font-weight: 500;\r\n    color: #01151e;\r\n    font-size: 25px;\r\n}\r\n.pwat-product__details-kit-submit {\r\n    width: fit-content;\r\n    padding: 0 50px;\r\n    height: 50px;\r\n    background: #01151e;\r\n    color: #fff;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    font-size: 14px;\r\n    transition: all .5s;\r\n    border-radius: 30px;\r\n    margin: 0 auto;\r\n    margin-top: 30px;\r\n}\r\n.pwat-product__details-kit-submit:hover {\r\n    background: #ebebeb;\r\n    color: #01151e;\r\n    transition: all .5s;\r\n}\r\n\r\n@media (max-width: 1024px) {\r\n    /**\r\n    * PÁGINA DE PRODUTO\r\n    */\r\n    .pwat-product {\r\n        margin-top: 60px;\r\n    }\r\n\r\n    .pwat-breadcumb {\r\n        padding-top: 20px !important;\r\n    }\r\n\r\n    /**\r\n    *  class .pwat-gallery-*\r\n    */\r\n    .pwat-gallery {\r\n        padding: 10px;\r\n        margin-top: 25px;\r\n        display: flex;\r\n        flex-flow: column;\r\n    }\r\n\r\n    .pwat-gallery__main-image {\r\n        width: 100%;\r\n    }\r\n\r\n    .pwat-gallery__imagens {\r\n        width: 100%;\r\n    }\r\n\r\n    .pwat-gallery__info {\r\n        margin-left: 0;\r\n        width: calc(100% - 20px);\r\n        padding: 10px;\r\n        margin-top: 15px;\r\n    }\r\n\r\n    .pwat-gallery__info-title {\r\n        font-size: 18px;\r\n    }\r\n\r\n    .pwat-gallery__info-submit {\r\n        width: 100%;\r\n    }\r\n\r\n    /**\r\n    *  class .pwat-input__number-*\r\n    */\r\n    .pwat-input__number {\r\n        width: 120px;\r\n    }\r\n\r\n    .pwat-input__number button {\r\n        width: 40px;\r\n    }\r\n\r\n    .pwat-input__number input {\r\n        width: 40px;\r\n    }\r\n\r\n    .pwat-gallery__submit-button {\r\n        margin-left: 0px;\r\n        font-size: 11px;\r\n    }\r\n\r\n    /**\r\n    *  class .pwat-product__details-*\r\n    */\r\n    .pwat-product__details {\r\n        padding: 10px;\r\n        font-size: 13px;\r\n    }\r\n\r\n\r\n    /**\r\n    *  class .pwat-product__details-kit\r\n    */\r\n    .pwat-product__details-kit-product {\r\n        width: calc(50% - 20px);\r\n    }\r\n\r\n    .pwat-product__details-kit-product:not(:last-child) {\r\n        width: calc(50% - 30px);\r\n    }\r\n}"},{"metadata":"js","name":"__custom","extension":"javascript","content":"/**\n * Códigos JS\n */\nconsole.log('cli 3')"},{"metadata":"js","name":"__javascript_maisum","extension":"javascript","content":"console.log('cli 4')"},{"metadata":{"title":"Sobre Nós","description":"Saiba mais sobre a principal marca Nacional, consagrada pelo seu Conceito único no país.","keywords":"lion fitness, lion, fitness, aparelhos de academia, aparelhos para academia, aparelhos, academia, equipamentos de academia, equipamentos para academia, equipamentos, musculacao, profissional, empresa, fabrica, marca fitness"},"name":"sobre-nos","extension":"html","content":"<main>\n    <h1 style=\"display: none\">Sobre Nós</h1>\n    <!-- \n        Sessão sobre a empresa, HTML e CSS básico, apenas para demonstração de uma página customizada\n        As páginas customizadas podem ser criadas através do botão \"Salvar Nova\" acima das páginas listadas\n        no menu à sua esquerda.\n     -->\n    <section class=\"pwat-page__about-us\">\n        <h2>Sobre Nós</h2>\n        <p>There are many variations of passages of Lorem Ipsum available, \n            but the majority have suffered alteration in some form, by injected humour, \n            or randomised words which don't look even slightly believable.\n            If you are going to use a passage of Lorem Ipsum, you need to be \n            sure there isn't anything embarrassing hidden in the middle of text. \n            All the Lorem Ipsum generators on the Internet tend to repeat predefined\n             chunks as necessary, making this the first true generator on the Internet. \n             It uses a dictionary of over 200 Latin words, combined with a handful of model sentence structures, \n             to generate Lorem Ipsum which looks reasonable. The generated Lorem Ipsum is therefore always \n             free from repetition, injected humour, or non-characteristic words etc.</p>\n        <p><img style=\"display: block; margin-left: auto; margin-right: auto;\" alt=\"PWA\" pwa-src=\"logo\" /></p>\n        <p>&nbsp;</p>\n        <p><b>Sobre PWAs</b></p>\n        <p>&nbsp;</p>\n        <div class=\"pwat-video__about-us\"><iframe src=\"https://www.youtube.com/embed/z2JgN6Ae-Bo\" height=\"240\"\n                width=\"320\"></iframe></div>\n        <p>&nbsp;</p>\n        <p><em><b>#PWA #PWA4ALL</b></em></p>\n    </section>\n</main>"},{"metadata":null,"name":"__cart","extension":"html","content":"<section id=\"pwat-cart\" class=\"pwat-cart\">\n    <div>\n        <header class=\"pwat-cart__header\">\n            <a pwa-toggle-cart>\n                <span class=\"lnr lnr-cross\" aria-hidden=\"true\"></span>\n            </a>\n\n            <div>\n                <h2>Meu Carrinho</h2>\n                <span pwa-has-items>({{ $$cart.count }})</span>\n            </div>\n        </header>\n    </div>\n\n    <div class=\"pwat-cart__free-shipping-warning\">\n        <!-- BACKGROUND FREIGHT -->\n        <div class=\"pwat-cart__fill-progress\" pwa-style=\"{ width:  `${(($$cart.subtotal*100)/150 )}%`}\"></div>\n\n        <p pwa--if=\"150.00 - $$cart.subtotal  > 0\" class=\"pwat-cart__quant-freight-free\">\n            Faltam apenas <span class=\"pwat-text-bold\">{{ (150.00 - $$cart.subtotal )| price }}</span> para você ganhar <span class=\"pwat-text-bold\">frete grátis</span>\n        </p>\n        <p pwa--if=\"150.00 - $$cart.subtotal  <= 0\" class=\"pwat-cart__quant-freight-free\">Você ganhou frete grátis!</span></p>\n    </div>\n\n    <section pwa-not-items class=\"pwat-cart__empty\">\n        <i class=\"lnr lnr-heart\" aria-hidden=\"true\"></i>\n        Seu carrinho está vazio<br> \n        porque você ainda não viu as novidades.\n        <a id=\"pwat-close-cart\" pwa-to-search=\"Men\" pwa-hook.click=\"close-cart\">Ver novidades</a>\n    </section>\n\n    <!-- \n        Lista de produtos no carrinho, verifica se o carrinho possui produtos adicionados (pwa-has-items);\n        Em seguida, chama produto por produto através da tag (pwa-each-item), assim você consegue ter acesso ao objeto \"product\" para (product.name) e etc..\n     -->\n    <ul pwa-has-items class=\"pwat-cart__list\">\n        <li pwa-each-item class=\"pwat-cart__item\">\n            <section class=\"pwat-cart__row-bag\">\n                <div class=\"pwat-cart__row-bag-container\">\n                    <figure class=\"pwat-cart__product-img\">\n                        <img pwa-src=\"product.image\" pwa-alt=\"product.name\">\n                    </figure>\n\n                    <article class=\"pwat-cart__row-pay\">\n                        <h3>{{ product.name }}</h3>\n                        \n                        <!-- Opções do produto  (Utilizados em projetos MAGENTO 2)-->\n                        <ul pwa--if=\"(product.options || {}).attributes_info\" class=\"pwat-cart__options\">\n                            <li pwa--for=\"option in Object.values((product.options || {}).attributes_info || {})\"\n                                pwa-key=\"option.value\" ><strong>{{ option.label }}:</strong> {{ option.value }}</li>\n                        </ul>\n\n                        <ul pwa--if=\"(product.options || {}).options\" class=\"pwat-cart__options\">\n                            <li pwa--for=\"option in Object.values((product.options || {}).options || {})\" pwa-key=\"option.value\">\n                                <strong>{{ option.label }}:</strong> {{ option.value }}</li>\n                        </ul>\n\n                        <section>\n                            <div class=\"pwat-input__number\">\n                                <span pwa-quantity-decrement class=\"pwat-input__number-decrement\">–</span>\n                                <input pwa-quantity-box class=\"input-number\">\n                                <span pwa-quantity-increment class=\"pwat-input__number-increment\">+</span>\n                            </div>\n\n                            <small class=\"pwat-cart__product-oldprice\" pwa--if=\"(product.price - product.discount) > 0 && product.discount > 0\">{{ product.price | price }}</small>\n                            <small pwa--if=\"(product.price - product.discount) > 0\">{{ (product.price - product.discount) | price }}</small>\n                            <small pwa--if=\"(product.price - product.discount) <= 0\">Grátis</small>\n                        </section>\n                    </article>\n                </div>\n\n                <a pwa-remove-item class=\"pwat-cart__delete-item\">\n                    <i class=\"lnr lnr-trash\"></i>\n                </a>\n            </section>\n        </li>\n    </ul>\n\n    <!-- Loading - usado para aguardar os produtos carregarem no carrinho -->\n    <div pwa-is-loading class=\"pwat-cart__list\">\n        <pwa-skeleton w=\"100%\" h=\"80px\" size=\"3\">\n            <pwa-skeleton-block h=\"80px\" w=\"80px\"/>\n            \n            <div class=\"pwat-cart__skeleton-col\">\n                <pwa-skeleton-block w=\"40%\" h=\"30px\"/>\n                <pwa-skeleton-block h=\"30px\"/>\n            </div>\n        </pwa-skeleton>\n    </div>\n\n    <!-- \n        Rodapé do carrinho, responsável pelo resumo do pedido e CTA de finalizar pedido.\n        Existem duas outras sections comentada caso o projeto seja E-commerce completo e não apenas orçamento.\n     -->\n    <div>\n        <div pwa-has-items class=\"pwat-cart__footer\">\n            <div class=\"pwat-cart__subtotal\">\n                <h3>Subtotal</h3>\n                <span class=\"pwat-cart__subtotal_flex-end\">{{ $$cart.subtotal | price }}</span>\n            </div>\n\n            <div class=\"pwat-cart__discout\">\n                <div class=\"pwat-form__input\">\n                    <input pwa-freight-box required placeholder=\"99999-999\" id=\"discount-cart\" />\n                    <button pwa-freight-submit class=\"pwat-form__btn\">Buscar</button>\n                    \n                    <!-- Loading - enquanto o frete é buscado -->\n                    <pwa-hidden>\n                        <div class=\"pwat-loading pwat-form__btn\" pwa-freight-loading>\n                            <svg viewBox=\"0 0 90 90\" xmlns=\"http://www.w3.org/2000/svg\">\n                                <circle id=\"pwat-loading\" fill=\"none\" stroke-width=\"4\" stroke-linecap=\"round\" stroke=\"#ccc\" cx=\"45\" cy=\"45\" r=\"43\" />\n                            </svg>\n                        </div>\n                    </pwa-hidden>\n                </div>\n\n                <span pwa-each-freight class=\"pwat-freight__result\">\n                    <small pwa--if=\"freight.price > 0\">Frete: {{ freight.price | price }}</small>\n                    <small pwa--if=\"freight.price <= 0\">Frete: Grátis</small>\n                </span>\n\n                <div class=\"pwat-form__input\">\n                    <input pwa-coupon-box required placeholder=\"ex.: LOJA123\" id=\"discount-cart\" />\n                    <button pwa-coupon-apply pwa-without-coupon class=\"pwat-form__btn\">Aplicar</button>\n\n                    <span pwa-with-coupon class=\"pwat-form__input-checked\">✓</span>\n                </div>\n            </div>\n\n            <section pwa-with-coupon class=\"pwat-cart__discout--success\">\n                <h3>Desconto</h3>\n                <span>-{{ $$cart.discount | price }}</span>\n            </section>\n\n            <section class=\"pwat-cart__total\">\n                <h3>Total</h3>\n                <span>{{ $$cart.total | price }}</span>\n            </section>\n            \n            <!-- pwa-checkout ( Envia o pedido para o checkout da loja ou para o DSC (Checkout Integrado à PWA) ) -->\n            <div pwa-has-items class=\"pwat-cart__btn\">\n                <a  pwa-checkout>\n                    FINALIZAR PEDIDO\n                </a>\n            </div>\n        </div>\n    </div>\n</section>"},{"metadata":null,"name":"__category","extension":"html","content":"<main class=\"pwat-category\">\r\n\r\n    <!-- Breadcumb -->\r\n\t<div class=\"pwat-breadcumb pwat-container\">\r\n        <a pwa-to-page=\"home\" class=\"pwat-breadcumb__item\">Home</a>\r\n\t\t<a pwa-breadcumb-item class=\"pwat-breadcumb__item\">{{ bc.name }}</a>\r\n\t</div>\r\n\r\n    <!-- Select de buscas padronizado para buscas no elasticsearch -->\r\n\t<section class=\"pwat-filter pwat-container\">\r\n\t\t<div class=\"pwat-filter__submit\">\r\n            <h2>ORDENAR POR:</h2>\r\n            <pwa-manual-filter>\r\n                <select class=\"pwat-filter__select\" pwa-filter-input name=\"order\" id=\"order\">\r\n                    <option value=\"none\">Todos</option>\r\n                    <option value=\"name:+\">Nome</option>\r\n                    <option value=\"price:-\">Maior Preço</option>\r\n                    <option value=\"price:+\">Menor Preço</option>\r\n                </select>\r\n            </pwa-manual-filter>\r\n\t\t</div>\r\n\t</section>\r\n\r\n    <!-- Sessão dos produtos encontrados -->\r\n    <div class=\"pwat-results pwat-container\">\r\n\r\n        <!-- Botão de filtrar somente para o mobile -->\r\n        <a class=\"pwat-filters__funnel ismobile\" pwa-hook.click=\"category-filters-toggle-mobile\"><span class=\"lnr lnr-funnel\"></span></a>\r\n        <div class=\"pwat-filters__background-close\" pwa-hook.click=\"category-filters-toggle-mobile\"></div>\r\n\r\n        <!-- FILTROS -->\r\n\t\t<div class=\"pwat-filters\">\r\n            <a class=\"pwat-filters__close ismobile\" pwa-hook.click=\"category-filters-toggle-mobile\"><span class=\"lnr lnr-cross\"></span></a>\r\n\r\n            <pwa-tags-filter>\r\n                <!-- FILTERS ACTIVE -->\r\n                <section class=\"pwat-filters__founds\">\r\n                    <div class=\"pwat-filters__founds-header\">\r\n                        <h2 class=\"pwat-filters__founds-title\">Filtros Ativos</h2>\r\n                    </div>\r\n\r\n                    <div pwa-not-filters class=\"pwat-not__filters\">\r\n                        <p>Nenhum filtro ativo!</p>\r\n                    </div>\r\n\r\n                    <div pwa-has-filters class=\"pwat-filter__found\">\r\n                        <div pwa-each-filter class=\"pwat-filters__founds-item\">\r\n                            <div class=\"pwat-filters__active-item\">\r\n                                <p class=\"pwat-filters__active-item-label\">{{ filter.value }}</p>\r\n                                <a pwa-remove-filter=\"click\"><span class=\"lnr lnr-cross\"></span></a>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <p pwa-has-filters pwa-clean-filters class=\"pwat-clean-filters pwat-text-center\">Limpar todos os filtros</p>\r\n                </section>\r\n            </pwa-tags-filter>\r\n\r\n            <!-- \r\n                FILTRO SUBCATEGORIAS\r\n                parâmetros: \r\n                    - pwa-parent: Através do id da categoria listada na página, são buscados todos os seus filhos/subcategorias.\r\n                    - size: limite máximo de subcategorias a serem listados\r\n            -->\r\n            <pwa-category-list pwa-parent=\"category.id\" size=\"4\" class=\"pwat-filters__founds pwat-filters__founds-anchor\">\r\n                <a class=\"pwat-filters__founds-header\" pwa-hook.click=\"show-and-hide-founds-filter\">\r\n                    <h2 class=\"pwat-filters__founds-title\">Categorias</h2>\r\n                    <span pwa-has-category>+</span>\r\n                </a>\r\n\r\n                <small pwa-list-empty class=\"pwat-filters__not-founds-header\"> \r\n                    A categoria buscada não possui subcategorias.\r\n                </small>\r\n\r\n                <div id=\"pwat-filters__founds-categorias\" class=\"pwat-filters__founds-container\" pwa-has-category>\r\n                    <div pwa-each-category class=\"pwat-filters__founds-wrapper\">\r\n                        <a pwa-to-category-page class=\"pwat-filters__founds-item\">\r\n                            {{ category.name }}\r\n                        </a>\r\n                    </div>\r\n                </div>\r\n            </pwa-category-list>\r\n\r\n            <!-- \r\n                FILTRO CORES\r\n                parâmetros: \r\n                    - color:   é passado o nome do atributo no qual deseja buscar do elastic para que seja filtrado. No exemplo abaixo\r\n                                foi passado o atributo \"linha\" que se encontra em \"specifications.linha\", por padrão, a PWA já reconhece que os atributos dentro de\r\n                                specifications são filtráveis, porém se o atributo linha estiver fora de specifications e estiver na raiz onde o mesmo não possui nenhum \r\n                                atributo pai,  PWA também vai reconhecer normalmente. Qualquer outro atributo fora de specifications, é necessário passar o seu \"pai\"\r\n                                como exemplo: \"pai.atributo\".\r\n\r\n                    - size: limite máximo de subcategorias a serem listados\r\n            -->\r\n            <pwa-variation-filter code=\"color\" multiple class=\"pwat-filters__founds pwat-filters__founds-anchor pwat-filters__color\">\r\n                <a class=\"pwat-filters__founds-header\" pwa-hook.click=\"show-and-hide-founds-filter\">\r\n                    <h2 class=\"pwat-filters__founds-title\">Cores</h2>\r\n                    <span pwa-has-filters>+</span>\r\n                </a>\r\n\r\n                <small pwa-not-filters class=\"pwat-filters__not-founds-header\"> \r\n                    Esses produtos não possuem opções de cores.\r\n                </small>\r\n\r\n                <div pwa-has-filters class=\"pwat-filters__founds-container\">\r\n                    <label pwa-each-filter class=\"pwat-filters__founds-wrapper\">\r\n                        <span pwa-filter-input=\"data-checked\" class=\"pwat-filters__founds-item\">\r\n                            <span class=\"\" :style=\"{ backgroundColor: (filter || {}).name }\"></span>[{{ filter.count }}]\r\n                        </span>\r\n                    </label>\r\n                </div>\r\n            </pwa-variation-filter>\r\n\r\n            <!-- FILTRO TAMANHOS  -->\r\n            <pwa-variation-filter code=\"size\" multiple class=\"pwat-filters__founds pwat-filters__founds-anchor pwat-filters__size\">\r\n                <a class=\"pwat-filters__founds-header\" pwa-hook.click=\"show-and-hide-founds-filter\">\r\n                    <h2 class=\"pwat-filters__founds-title\">Tamanhos</h2>\r\n                    <span pwa-has-filters>+</span>\r\n                </a>\r\n\r\n                <small pwa-not-filters class=\"pwat-filters__not-founds-header\"> \r\n                    Produtos de tamanho único.\r\n                </small>\r\n\r\n                <div pwa-has-filters class=\"pwat-filters__founds-container\">\r\n                    <label pwa-each-filter class=\"pwat-filters__founds-wrapper\">\r\n                        <span pwa-filter-input=\"data-checked\" class=\"pwat-filters__founds-item\">\r\n                            {{ filter.label }} [{{ filter.count }}]\r\n                        </span>\r\n                    </label>\r\n                </div>\r\n            </pwa-variation-filter>\r\n\r\n            <!-- FILTRO POR RANGE DE PREÇO  -->\r\n            <pwa-price-filter max-size=\"5\" class=\"pwat-filters__founds pwat-filters__founds-anchor pwat-filters__price\">\r\n                <a class=\"pwat-filters__founds-header\" pwa-hook.click=\"show-and-hide-founds-filter\">\r\n                    <h2 class=\"pwat-filters__founds-title\">Range de Preço</h2>\r\n                    <span>+</span>\r\n                </a>\r\n\r\n                <div  class=\"pwat-filters__founds-container\">\r\n                    <div class=\"pwat-filters__founds-container-form\">\r\n                        <input  pwa-range-min-input  type=\"number\" name=\"price-from\"  placeholder=\"min\" class=\"efs-filtro-option-input\" >\r\n                        <small>até</small>\r\n                        <input pwa-range-max-input  type=\"number\" name=\"price-to\"  placeholder=\"max\" class=\"efs-filtro-option-input\" >\r\n                        <button pwa-range-submit type=\"button\"> Filtrar </button>\r\n                    </div>\r\n                </div>\r\n            </pwa-price-filter>\r\n\r\n\t\t</div>\r\n\r\n        <!-- Resultados e listagem dos produtos -->\r\n\t\t<div class=\"pwat-results__items\">\r\n\t\t\t<span class=\"pwat-results__quantity\">{{ $$paginate.count }} resultados</span>\r\n\r\n            <!-- \r\n                pwa-result-list traz a lista dos produtos encontrados\r\n                params = [\r\n                    9 => 'Quantidade de produtos listado por página',\r\n                    2 => 'Scroll para de carregar automáticamente ná página 2, da página três em diante, é necessário que o cliente clique em \"carregar mais..\" '\r\n                ]\r\n             -->\r\n\t\t\t<pwa-result-list size=\"24\" automatic-scrolls=\"0\" class=\"pwat-results__products\">\r\n\t\t\t\t<span pwa-not-result style=\"padding: 100px 0px; text-align: center; width: 100%\">Sem resultados para mostrar</span>\r\n\r\n                <div class=\"pwat-results__list\">\r\n\t\t\t\t\t<div pwa-each-product class=\"pwat-results__item\">\r\n\t\t\t\t\t\t<pwa-product-card>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\r\n                 <!-- Loading em execução até que se encontre os produtos -->\r\n                <div pwa-is-loading class=\"pwat-results__list\">\r\n                    <pwa-skeleton w=\"33%\" h=\"500px\" size=\"24\">\r\n                        <pwa-skeleton-block h=\"350px\"/>\r\n                        <pwa-skeleton-block w=\"40%\" h=\"30px\"/>\r\n                        <pwa-skeleton-block h=\"30px\"/>\r\n                    </pwa-skeleton>\r\n                </div>\r\n\r\n\t\t\t\t<div class=\"pwat-results_totop\">\r\n\t\t\t\t\t<a href=\"#\"><span class=\"lnr lnr-chevron-up\"></span></a>\r\n\t\t\t\t</div>\r\n\r\n                <!-- Botão carregar mais produtos.. -->\r\n\t\t\t\t<div pwa-not-last-page class=\"pwat-results__loading-more\">\r\n\t\t\t\t\t<a pwa-not-loading pwa-load-more><i class=\"lnr lnr-chevron-down\"></i>\r\n\t\t\t\t\t\tCarregar mais</a>\r\n\t\t\t\t</div>\r\n\t\t\t</pwa-result-list>\r\n\t\t</div>\r\n  </div>\r\n</main>"},{"metadata":null,"name":"__display_rules","extension":"json","content":"{\n}"},{"metadata":null,"name":"__footer","extension":"html","content":"<footer class=\"pwat-footer\" id=\"pwat-footer\">\n\n    <!-- Formulário newsletter -->\n\t<pwa-newsletter class=\"pwat-footer__register pwat-container\">\n\t\t<h2>Cadastre-se e receba novidades no seu e-mail</h2>\n\t\t<div class=\"pwat-form\">\n\t\t\t<div class=\"pwat-form__input\">\n\t\t\t\t<input pwa-name-box placeholder=\"Nome\">\n\t        </div>\n\t\n            <div class=\"pwat-form__input\">\n                <input pwa-email-box placeholder=\"Email\">\n            </div>\n\t\n            <button  pwa-not-loading type=\"submit\" class=\"pwat-button__submit\">Cadastrar</button>\n\t\t</div>\n\t</pwa-newsletter>\n\n    <!-- Informações da Empresa -->\n    <div class=\"pwat-footer__info pwat-container\">\n        <img pwa-src=\"logo\" alt=\"Logo\">\n\n        <section class=\"pwat-footer__info-detail\">\n            <h3 class=\"pwat-footer__info-title\" pwa-hook.click=\"footer-open-items:footer-about\">\n                Institucional\n                <i class=\"lnr lnr-chevron-down ismobile\" aria-hidden=\"true\"></i>\n            </h3>\n\n            <div class=\"pwat-footer__info-content\" id=\"footer-about\">\n                <a pwa-to-page=\"sobre-nos\" >Sobre Nós</a>\n                <a href=\"#\" >Opção 1</a>\n                <a href=\"#\" >Opção 2</a>\n            </div>\n        </section>\n\n        <!-- Informações de categorias destaques -->\n        <section class=\"pwat-footer__info-detail\">\n            <h3 class=\"pwat-footer__info-title\" pwa-hook.click=\"footer-open-items:footer-products\">\n                Produtos\n                <i class=\"lnr lnr-chevron-down ismobile\" aria-hidden=\"true\"></i>\n            </h3>\n\n            <div class=\"pwat-footer__info-content\" id=\"footer-products\">\n                <a pwa-to-page=\"/women\">Women</a>\n                <a pwa-to-page=\"/men\">Men</a>\n                <a pwa-to-page=\"/gear\" >Gear</a>\n                <a pwa-to-page=\"/training\">Training</a>\n            </div>\n        </section>\\\n\n        <!-- Informações para contato da empresa -->\n        <section class=\"pwat-footer__info-detail\">\n            <h3 class=\"pwat-footer__info-title\" pwa-hook.click=\"footer-open-items:footer-contact\">\n                Contato\n                <i class=\"lnr lnr-chevron-down ismobile\" aria-hidden=\"true\"></i>\n            </h3>\n\n            <div class=\"pwat-footer__info-content\" id=\"footer-contact\">\n                <p>SAC <span class=\"pwat-footer__info-sac--number\">(99) 99999-9999</span></p>\n                <br>\n                <p>Segunda a Sexta, das 8h às 18h</p>\n            </div>\n        </section>\n\n        <!-- Informações de endereço da empresa -->\n        <section class=\"pwat-footer__info-detail\">\n            <h3 class=\"pwat-footer__info-title\" pwa-hook.click=\"footer-open-items:footer-address\">\n                Onde Estamos\n                <i class=\"lnr lnr-chevron-down ismobile\" aria-hidden=\"true\"></i>\n            </h3>\n\n            <div class=\"pwat-footer__info-content\" id=\"footer-address\">\n                <p>Endereço <br> Cidade - Estado <br> CEP</p>\n            </div>\n        </section>\n\n        <!-- Redes Sociais -->\n        <div class=\"pwat-footer__info-detail pwat-footer__info-social\">\n            <a href=\"#\" target=\"_blank\"> <span class=\"fa fa-instagram\"></span> </a>\n            <a href=\"#\" target=\"_blank\"> <span class=\"fa fa-facebook\"></span></a>\n            <a href=\"#\" target=\"_blank\"> <span class=\"fa fa-youtube\"></span></a>\n        </div>\n    </div>\n\n    <!-- Direitos Reservados -->\n    <div class=\"pwat-footer__last-info\">\n        <p>Increazy ® - Direitos Reservados </p>\n    </div>\n</footer>"},{"metadata":null,"name":"__header","extension":"html","content":"<header class=\"pwat-header pwat-container\" id=\"pwat-header\">\n    \n    <!-- Div que escurece a página quando abre o carrinho e também responsável pelo fechamento do carrinho -->\n\t<div class=\"pwat-cart__darken\" id=\"pwat-cart__darken\" pwa-toggle-cart></div>\n\n\t<!-- Chamada do Componente carrinho que fica no arquivo \"Carrinho\" -->\n    <pwa-cart open-on-add/>\n\n    <!-- Hamburger icon do Mobile -->\n    <div class=\"pwat-header__menu-mobile ismobile\">\n        <div class=\"pwat-hamburger\" pwa-hook.click=\"header-open-menu-mobile\">\n            <span></span>\n            <span></span>\n            <span></span>\n        </div>\n\n        <i class=\"lnr lnr-magnifier\" pwa-hook.click=\"header-open-search\"></i>\n    </div>\n\n    <!-- Logo principal no header -->\n    <figure pwa-to-page=\"home\" class=\"pwat-header__logo\">\n        <img pwa-src=\"logo\">\n    </figure>\n\n    <!-- Chamada da Categoria do back-end -->\n    <pwa-category-list class=\"pwat-header__category\" level=\"2\" name=\"main-menu\" size=\"10\"\n      id=\"pwat-header__category\">\n      <ul class=\"pwat-header__category-ul\" id=\"pwat-header__category-ul\">\n        <pwa-loop params=\"11\">\n            <li pwa-each-category \n                pwa-hook.mouseleave=\"header-close-submenu\"\n                class=\"pwat-header__category-li\"\n            >\n                <a \n                    class=\"isdesktop pwat-header__category-main\"\n                    pwa-show-list.mouseover=\"sub-main-menu\"\n                    pwa-hook.mouseover=\"header-open-submenu\"\n                    pwa-hook.click=\"header-close-submenu\"\n                    pwa-to-category-page\n                >\n                    {{ category.name }}\n                </a>\n\n                <a pwa-to-category-page \n                    pwa-hook.click=\"header-close-submenu\"\n                    class=\"ismobile pwat-header__category-main\">\n                    {{ category.name }}\n                </a>\n\n                <i class=\"lnr lnr-chevron-down ismobile\" aria-hidden=\"true\"  pwa-show-list.click=\"sub-main-menu\" pwa-hook.click=\"header-open-submenu\"></i>\n\n                <!-- \n                    Subcategorias da categoria principal \n                    parâmetros:\n                        - name: nome do sub-menu referênciado no menu pricipal na tag acima\n                        - size: quantidade máxima de submenus listadas\n                -->\n                <pwa-category-list \n                    name=\"sub-main-menu\" \n                    size=\"10\" \n                    :class=\"{'pwat-header__submenu': true, 'pwat-header__submenu-validation': !$$globalState.hasItems}\"\n                    pwa-id=\"`pwat-header__submenu-${category.name}`\" >\n\n                    <a pwa-to-category-page  pwa-hook.click=\"header-close-submenu\"><h2 pwa-has-category class=\"pwat-header__submenu-title\">  {{ main_category.name }} </h2> </a>\n\n                    <div class=\"pwat-header__submenu-items\">\n                        <pwa-loop params=\"8\">\n                            <a \n                                pwa-hook.click=\"header-close-submenu\"\n                                class=\"pwat-header__submenu-item\"\n                                pwa-each-category\n                                pwa-to-category-page\n                            >\n                                {{ category.name }}\n                            </a>\n                        </pwa-loop>\n                    </div>\n\n                    <span class=\"pwat-header__back ismobile\" pwa-hook.click=\"header-close-submenu\"><i class=\"lnr lnr-chevron-left\"></i> voltar</span>\n                </pwa-category-list>\n          </li>\n        </pwa-loop>\n\n        <li> <a pwa-to-page=\"sobre-nos\" class=\"pwat-header__category-main\" pwa-hook.click=\"header-close-submenu\">Sobre Nós</a> </li>\n      </ul>\n    </pwa-category-list>\n\n    <!-- Sessão de informações como idioma e formulário de buscas -->\n    <div class=\"pwat-header__options\">\n      <div class=\"pwat-header__options-store\">\n        \n        <div class=\"pwat-header__user\" pwa-not-logged pwa-to-auth>\n            <span class=\"lnr lnr-user\"></span>\n            <small>Entrar</small>\n        </div>\n\n        <div class=\"pwat-header__user\" pwa-is-logged pwa-to-account>\n            <span class=\"lnr lnr-user\"></span>\n            <small> Olá, {{ $$user.firstname }}</small>\n        </div>\n\n        <!-- Icone do carrinho informando a quantidade -->\n        <div class=\"pwat-header__bag\">\n          <a pwa-toggle-cart>\n            <span class=\"lnr lnr-cart\" pwa-toggle-cart> </span>\n            <span class=\"pwat-bag__quantity\">{{ $$cart.count }}</span>\n          </a>\n        </div>\n\n        <!-- Icone de busca -->\n        <span class=\"lnr lnr-magnifier pwat-icon__search isdesktop\" pwa-hook.click=\"header-open-search\"></span>\n\n        <!-- Sugestões dos produtos encontrados pelo formulário de busca -->\n        <pwa-search-form size=\"5\" class=\"pwat-header__search-input\" id=\"pwat-header__search-input\" pwa-hook=\"header-search-form\">\n           <div class=\"pwat-header__search-input-close\" pwa-hook.click=\"header-open-search\"></div> \n           \n           <input pwa-search-box placeholder=\"Digite algo como: Esteira\" pwa-hook=\"header-close-search-key-press\">\n\n            <div pwa-has-suggest class=\"pwat-header__search-items\">\n                <article pwa-each-suggest class=\"pwat-header__search-item\"  pwa-hook.click=\"header-open-search\">\n                    <img pwa-src=\"suggest.image\" pwa-alt=\"suggest.name\">\n                    <div>\n                        <h2>{{ suggest.name }}</h2>\n                        <small pwa--if=\"suggest.activeSKU.sale_price > 0\">\n                        {{ suggest.activeSKU.sale_price | price }}</small>\n                        <small pwa--if=\"suggest.activeSKU.sale_price <= 0\">Produto esgotado</small>\n                    </div>\n                </article>\n            </div>\n\n            <a pwa-has-suggest pwa-search-button class=\"pwat-header__search-see-all\">Ver todos</a>\n        </pwa-search-form>\n      </div>\n    </div>\n</header>"},{"metadata":null,"name":"__home","extension":"html","content":"<main class=\"pwat-home\">\n    <h1 style=\"display:none\">Increazy Example</h1>\n    <!--  Banner Principal -->\n    <div class=\"pwat-main__banner\" pwa-hook=\"home-main-banner\">\n        <div class=\"swiper-container pwat-main__banner-slide\" id=\"pwat-main__banner-slide\">\n            <div class=\"swiper-wrapper\">\n                <a pwa-to-search=\"Men\" class=\"swiper-slide\">\n                    <img pwa-src=\"main-banner\" alt=\"Banner sobre INCREAZY\">\n                </a>\n\n                <a pwa-to-search=\"Woman\" class=\"swiper-slide\">\n                    <img pwa-src=\"main-banner\" alt=\"Banner sobre INCREAZY\">\n                </a>\n            </div>\n\n            <div class=\"swiper-button-next pwat-main__banner-slide__next\"></div>\n            <div class=\"swiper-button-prev pwat-main__banner-slide__prev\"></div>\n        </div>\n    </div>\n\n    <!--  Sessão de categorias abaixo do banner -->\n    <div class=\"pwat-main__categories\">\n        <article pwa-to-page=\"/women\" class=\"pwat-main__categories-card  pwat-hover__effect\">\n            <div class=\"pwat-ico-category-home pwat-category-cardio\"></div>\n            <h2>Women</h2>\n            <p>Lorem ipsum easy to use it</p>\n        </article>\n\n        <article pwa-to-page=\"/men\" class=\"pwat-main__categories-card pwat-hover__effect\">\n            <div class=\"pwat-ico-category-home pwat-category-bateria-de-peso\"></div>\n            <h2>Men</h2>\n            <p>Lorem ipsum easy to use it</p>\n        </article>\n\n        <article pwa-to-page=\"/gear\" class=\"pwat-main__categories-card pwat-hover__effect\">\n            <div class=\"pwat-ico-category-home pwat-category-peso-livre\"></div>\n            <h2>Gear</h2>\n           <p>Lorem ipsum easy to use it</p>\n        </article>\n\n        <article pwa-to-page=\"/training\" class=\"pwat-main__categories-card pwat-hover__effect\">\n            <div class=\"pwat-ico-category-home pwat-category-estacao\"></div>\n            <h2>Training</h2>\n           <p>Lorem ipsum easy to use it</p>\n        </article>\n    </div>\n\n    <!--  Sessão da listagem de produtos da categoria Classic -->\n    <div class=\"pwat-highlight pwat-container\" id=\"pwat-highlight\">\n        <section class=\"pwat-highlight__bigcard\">\n            <a pwa-to-search=\"Men\" class=\"pwat-highlight__bigcard-img\">\n                <img pwa-src=\"banner-category\" alt=\"Classic\">\n                <h2 id=\"pwat-highlight__title\">CLASSIC</h2>\n            </a>\n\n            <p>\n                Contrary to popular belief, Lorem Ipsum is not simply random text. It has roots in a piece of classical Latin literature from 45 BC, making it over 2000 years old.\n            </p>\n        </section>\n\n        <!--  Slide da sessão categoria Classic -->\n        <section class=\"pwat-highlight__products\">\n            <h2>Em destaque</h2>\n            <p>Conheça alguns produtos da linha CLASSIC</p>\n\n            <div class=\"swiper-button-next pwat-highlight__next\"></div>\n            <div class=\"swiper-button-prev pwat-highlight__prev\"></div>\n\n            <!--  \n                Listagem dos produtos da categoria no slide\n                    ====== PARÂMETROS =======\n                    filter = 'parametro de busca, pode ser categoria do produto, nome do produto e etc.. conforme os atributos do elastic da loja',  \n                    size = 'A quantidade de produtos exibidos no slide (padrão seria 15 produtos para não sobrecarregar a home page)',\n                    name = 'Nome dado para a chamada do hook ex. de nome de hook \"product-list-pwat-hightlight__products-slide-loaded\" \n                    esse é um nome padrão para todos os hooks que carregam os slides de produtos.. basta olhar em Hooks > Home'\n            -->\n            <pwa-product-list name=\"pwat-highlight__products-slide\" filter=\"categories.code:2\" size=\"12\" class=\"pwat-highlight__products-slide\">\n\n                <!--  Tag de loader caso os produtos demorem a carregar -->\n                <pwa-hidden>\n                    <div class=\"pwat-loading\" pwa-is-loading>\n                        <svg viewBox=\"0 0 90 90\" xmlns=\"http://www.w3.org/2000/svg\">\n                            <circle id=\"pwat-loading\" fill=\"none\" stroke-width=\"4\" stroke-linecap=\"round\" stroke=\"#ccc\" cx=\"45\" cy=\"45\" r=\"43\" />\n                        </svg>\n                    </div>\n                </pwa-hidden>\n\n                <!--  Inicio do conteúdo do slide - onde o swiper-container é o container principal do slide 2x2 -->\n                <div class=\"swiper-container pwat-highlight__slide\" id=\"pwat-highlight__slide\">\n                    <div pwa-not-loading class=\"swiper-wrapper pwat-highlight__wrapper\">\n                        <!--  \n                            Chamando cada produto do slide através da tag pwa-each-product: A classe Swiper-slide é atribuida como padrão \n                            do swiper para identificar que o card do produto (pwa-product-card) é um item do slide.\n                         -->\n                        <div pwa-each-product class=\"swiper-slide pwat-highlight__card-slide\">\n                            <pwa-product-card>\n                        </div>\n                    </div>\n                </div>\n            </pwa-product-list>\n        </section>\n    </div>\n\n    <!--  \n    Sessão de produtos da linha Lançamentos com slide 1x n \n    A ideia do slide segue a mesma abordada no Slide Classic\n    -->\n    <section class=\"pwat-releases pwat-container\" id=\"pwat-releases\">\n        <h2 class=\"pwat-releases__title\">\n            LANÇAMENTOS\n        </h2>\n\n        <pwa-product-list name=\"pwat-releases__products-slide\" filter=\"categories.code:3\" size=\"12\" class=\"pwat-releases__products-slide\">\n            <pwa-hidden>\n                <div class=\"pwat-loading\" pwa-is-loading>\n                    <svg viewBox=\"0 0 90 90\" xmlns=\"http://www.w3.org/2000/svg\">\n                        <circle id=\"pwat-loading\" fill=\"none\" stroke-width=\"4\" stroke-linecap=\"round\" stroke=\"#ccc\" cx=\"45\" cy=\"45\" r=\"43\" />\n                    </svg>\n                </div>\n            </pwa-hidden>\n\n            <div class=\"swiper-container pwat-releases__slide\" id=\"pwat-releases__slide\">\n                <div pwa-not-loading class=\"swiper-wrapper pwat-releases__wrapper\">\n                    <div pwa-each-product class=\"swiper-slide pwat-releases__card-slide\">\n                        <pwa-product-card>\n                    </div>\n                </div>\n\n                <div class=\"swiper-pagination pwat-releases__pagination\"></div>\n            </div>\n        </pwa-product-list>\n    </section>\n\n    <!-- Sessão de informação adicional da empresa -->\n    <div class=\"pwat-company pwat-container\" id=\"pwat-company\">\n        <article class=\"pwat-company__content\">\n            <h2>A INCREAZY</h2>\n            <p>\n                Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has\n                survived not only five centuries, but also the leap into electronic typesetting, remaining essentially unchanged. It was popularised in the 1960s with the release of Letraset sheets containing Lorem Ipsum passages, and more recently with\n                desktop publishing software like Aldus PageMaker including versions of Lorem Ipsum.\n            </p>\n        </article>\n\n        <img pwa-src=\"logo\" alt=\"INCREAZY\">\n    </div>\n</main>"},{"metadata":null,"name":"__product","extension":"html","content":"<main class=\"pwat-product\">\n    <!-- Breadcumb -->\n    <div class=\"pwat-breadcumb pwat-container\">\n        <a pwa-to-page=\"home\" class=\"pwat-breadcumb__item\">Home</a>\n        <a pwa-breadcumb-item class=\"pwat-breadcumb__item\">{{ bc.name }}</a>\n        <a class=\"pwat-breadcumb__item\">{{ product.name }}</a>\n    </div>\n\n    <!-- Sessão de exibição do produto -->\n    <div class=\"pwat-container\" pwa-hook.mouseenter=\"zoomout\">\n        <div class=\"pwat-gallery\">\n            <div class=\"pwat-gallery__imagens\" pwa-hook=\"swiper-gallery-product\">\n\n                <!-- slide Imagem principal do produto -->\n                <div class=\"pwat-gallery__top gallery-top swiper-container\" id=\"pwat-gallery__top\">\n                    <div class=\"swiper-wrapper\">\n                        <pwa-loop params=\"5\">\n                            <figure pwa-each-image class=\"swiper-slide\">\n                                <img pwa-src=\"image\" pwa-alt=\"product.name\" class=\"pwat-gallery__main-image\" :data-zoom-image=\"image\">\n                            </figure>\n                        </pwa-loop>\n                    </div>\n\n                    <div pwa-hook.mouseenter=\"zoomin\" class=\"pwat-gallery__view-zoom\" pwa-is-desktop> \n                        <img src=\"https://image.flaticon.com/icons/png/512/117/117453.png\" alt=\"Zoom\">\n                    </div>\n                </div>\n\n                <!-- Miniaturas das imagens de exibição do produto -->\n                <div class=\"pwat-gallery__list-images swiper-container gallery-thumbs\" id=\"pwat-gallery__list-images\">\n                    <div class=\"swiper-wrapper\">\n                        <pwa-loop params=\"5\">\n                            <a pwa-each-image class=\"swiper-slide\">\n                                <img pwa-src=\"image\" pwa-alt=\"product.name\">\n                            </a>\n                        </pwa-loop>\n                    </div>\n                </div>\n            </div>\n\n\n            <!-- Informações do produto -->\n            <section class=\"pwat-gallery__info\">\n\n                <!-- Adicionar produto aos favoritos  -->\n                 <div pwa-not-logged class=\"pwat-card__favorite-container\">\n                    <button pwa-to-auth class=\"pwat-card__favorite\">\n                    <img pwa-src=\"favorite\" alt=\"Favorito\">\n                    </button>\n                </div>\n\n                <div pwa-is-logged class=\"pwat-card__favorite-container\">\n                    <button pwa-add-wishlist class=\"pwat-card__favorite\">\n                    <img pwa-src=\"favorite\" alt=\"Favorito\">\n                    </button>\n\n                    <button pwa-remove-wishlist  class=\"pwat-card__favorite\">\n                        <img pwa-src=\"favorite-checked\" alt=\"Favorito\">\n                    </button>\n                </div>\n                <!-- final - Adicionar produto aos favoritos  -->\n\n                <h2 class=\"pwat-gallery__info-title\">{{ product.name }}</h2>\n                <span class=\"pwat-gallery__info-sku\">SKU: {{ product.sku }}</span>\n\n                <div class=\"pwat-gallery__info-price\">\n                    <span pwa-with-discount class=\"discount\">{{ activeSKU.old_price | price }}</span>\n\n                    <span pwa--if=\"activeSKU.sale_price > 0\" class=\"price\">\n                        <p>{{ activeSKU.sale_price | price }}</p>\n                        <small pwa--if=\"activeSKU.plots_number\"><strong>ou {{ activeSKU.plots_number }}x</strong> {{ activeSKU.plots_price | price }}</small>\n                    </span>\n\n                    <span pwa--if=\"activeSKU.sale_price <= 0\" class=\"price\">\n                        <p>Produto esgotado</p>\n                    </span>\n                </div>\n\n                <div class=\"pwat-gallery__configurations\">\n                    <pwa-product-configurations-bundle always-list attribute=\"variation_bundle\"></pwa-product-configurations-bundle>\n                    <pwa-product-configurations-bundle always-list attribute=\"custom_options\"></pwa-product-configurations-bundle>\n                </div>\n\n                <!-- Formulário de quantidade do item -->\n                <div class=\"pwat-gallery__info-submit\">\n                    <div class=\"pwat-input__number\">\n                        <button pwa-quantity-increment>+</button>\n                        <input pwa-quantity-box>\n                         <button pwa-quantity-decrement>-</button>\n                    </div>\n\n                    <a pwa-add-in-cart class=\"pwat-gallery__submit-button\">Adicionar ao Carrinho</a>\n                </div>\n\n                <!-- Compartilhamento do produto nas redes sociais.. -->\n                <div class=\"pwat-gallery__share\">\n                    <a class=\"pwat-gallery__share-facebook\"> <i class=\"fa fa-facebook\" aria-hidden=\"true\"></i> </a>\n                    <a class=\"pwat-gallery__share-twitter\"><i class=\"fa fa-twitter\" aria-hidden=\"true\"></i></a>\n                    <a class=\"pwat-gallery__share-linkedin\"><i class=\"fa fa-linkedin\" aria-hidden=\"true\"></i></a>\n                    <a class=\"pwat-gallery__share-email\"><i class=\"fa fa-envelope\" aria-hidden=\"true\"></i></a>\n                </div>\n\n                <!-- Descrição do produto -->\n                <section pwa--if=\"product.short_description\" class=\"pwat-gallery__description\">\n                    <h3>Descrição Rápida</h3>\n                    <p pwa--html=\"product.short_description\"></p>\n                </section>\n            </section>\n        </div>\n\n         <!-- Descrição completa do produto -->\n        <div class=\"pwat-product__details\">\n            <div pwa--html=\"product.description\"></div>\n        </div>\n\n        <!-- Produtos Kit  -->\n        <pwa-product-kit \n            attribute=\"relateds\" \n            all-required\n            class=\"pwat-product__kits\"\n        >\n            <h2 pwa-has-kit>Kits deste Produto</h2>\n\n            <div class=\"pwat-product__details\" pwa-has-kit>\n                <div pwa-not-empty class=\"pwat-product__details-kit\">\n                    <div pwa-each-kit  class=\"pwat-product__details-kit-product\">\n                        <strong class=\"\">{{ kit.name }}</strong>\n                        <img pwa-src=\"kit.image\"/>\n                        <pwa-product-configurations-bundle   attribute=\"variation_bundle\"></pwa-product-configurations-bundle>\n                    </div>\n                </div>\n\n                <a pwa-has-kit pwa-kit-submit class=\"pwat-product__details-kit-submit\">Adicionar Kit</a>\n            </div>\n\n        </pwa-product-kit>\n\n        <!--  Produtos Relacionados -->\n        <pwa-product-linkeds type=\"relateds\" name=\"relateds-example\">\n            <div class=\"pwat-product__linkeds pwat-product__linkeds-relateds\">\n                <h2> Produtos Relacionados </h2>\n                <div class=\"pwat-product__details\">\n                    <pwa-hidden>\n                        <div class=\"pwat-loading\" pwa-is-loading>\n                            <svg viewBox=\"0 0 90 90\" xmlns=\"http://www.w3.org/2000/svg\">\n                                <circle id=\"pwat-loading\" fill=\"none\" stroke-width=\"4\" stroke-linecap=\"round\" stroke=\"#ccc\" cx=\"45\" cy=\"45\" r=\"43\" />\n                            </svg>\n                        </div>\n                    </pwa-hidden>\n\n                    <div class=\"swiper-container pwat-product__linkeds-relateds-container\" id=\"pwat-product__linkeds-relateds-container\">\n                        <div pwa-not-loading class=\"swiper-wrapper\">\n                            <div pwa-each-linked class=\"swiper-slide\">\n                                <pwa-product-card>\n                            </div>\n                        </div>\n                        <div class=\"swiper-pagination pwat-product__linkeds-relateds-pagination\"></div>\n                    </div>\n\n                </div>\n            </div>\n        </pwa-product-linkeds>\n\n    </div>\n\n</main>"},{"metadata":null,"name":"__product_card","extension":"html","content":"<section class=\"pwat-card__product\">\n\n    <!-- \n        Adicionar e remover da lista de favoritos\n    -->\n    <div pwa-not-logged class=\"pwat-card__favorite-container\">\n        <button pwa-to-auth class=\"pwat-card__favorite\">\n           <img pwa-src=\"favorite\" alt=\"Favorito\">\n        </button>\n    </div>\n\n    <div pwa-is-logged class=\"pwat-card__favorite-container\">\n        <button pwa-add-wishlist class=\"pwat-card__favorite\">\n           <img pwa-src=\"favorite\" alt=\"Favorito\">\n        </button>\n\n        <button pwa-remove-wishlist  class=\"pwat-card__favorite\">\n            <img pwa-src=\"favorite-checked\" alt=\"Favorito\">\n        </button>\n    </div>\n\n    <!-- \n        pwa-to-product-page: Envia o cliente para a tela do produto clicado.\n    -->\n    <a pwa-to-product-page class=\"pwat-card__figure\">\n        <img class=\"pwat-card__product-img pwat-hover__effect\" pwa-src=\"product.image\" pwa-alt=\"product.name\">\n    </a>\n\n    <!-- \n        Informações do produto.\n        O Objeto \"product\" pode ser utilizado sem complicações dentro do carrinho.\n        Contando que todos os seus atributos existam no back-end.\n    -->\n    <article pwa-to-product-page class=\"pwat-card__info\">\n\t\t<h3 class=\"pwat-card__full-title\">{{ product.name }}</h3>\n\t\t\n        <p class=\"pwat-card__price\">\n            <span pwa-with-discount class=\"pwat-card__price-before\">{{ activeSKU.old_price | price }}</span>\n            <span pwa--if=\"!state.salePrice\" class=\"pwat-card__price-actual\">{{ activeSKU.sale_price | price }}</span>\n\t\t</p>\n\n        <p pwa--if=\"product.stock.inStock <= 0\"> Fora de estoque</p>\n\n\t\t<small class=\"pwat-card__quotes\">Em até  {{ product.installment.number }}x de {{ product.installment.value | price }}</small>\n\n        <!-- \n            CTA do card do produto\n            pwa-to-product-page: redireciona o cliente para a tela de produto\n\n            pwa-add-in-cart: manda direto o produto para o carrinho, sem a necessidade ir à página de produto para realizar essa ação.\n            obs.: Só vai funcionar para produtos simples, sem a necessidade de selecionar as opções de configuração (Tamanhos, cores e etc..)\n\n            Nessa caso, estamos usando apenas o pwa-to-product-page, pois existem produtos nessa loja que possuem configurações a serem escolhidas antes de ir\n            para o carrinho.\n        -->\n        <a pwa-to-product-page class=\"pwat-card__button\">\n            Comprar\n            <span class=\"lnr lnr-cart\"></span>\n        </a>\n\t</article>\n</section>"},{"metadata":null,"name":"__product_modal","extension":"html","content":"<div>\n\t<!--conteúdo aqui-->\n</div>"},{"metadata":null,"name":"__scripts_body","extension":"html","content":"<!-- LIB SLIDE SWIPER VERSION 6 -->\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/Swiper/5.4.5/js/swiper.min.js\" integrity=\"sha512-VHsNaV1C4XbgKSc2O0rZDmkUOhMKPg/rIi8abX9qTaVDzVJnrDGHFnLnCnuPmZ3cNi1nQJm+fzJtBbZU9yRCww==\" crossorigin=\"anonymous\"></script>\r\n"},{"metadata":null,"name":"__scripts_head","extension":"html","content":"<meta name=\"viewport\" content=\"width=device-width, user-scalable=no\">\n<link href=\"https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,800&display=swap\" rel=\"stylesheet\">\n<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.5.0/css/swiper.min.css\" />\n<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.11.1/css/lightbox.css\" />"},{"metadata":null,"name":"__search","extension":"html","content":"<main class=\"pwat-category\">\r\n\r\n    <!-- Select de buscas padronizado para buscas no elasticsearch -->\r\n\t<section class=\"pwat-filter pwat-container\">\r\n\t\t<div class=\"pwat-filter__submit\">\r\n            <h2>ORDENAR POR:</h2>\r\n            <pwa-manual-filter>\r\n                <select class=\"pwat-filter__select\" pwa-filter-input name=\"order\" id=\"order\">\r\n                    <option value=\"none\">Todos</option>\r\n                    <option value=\"name:+\">Nome</option>\r\n                    <option value=\"price:-\">Maior Preço</option>\r\n                    <option value=\"price:+\">Menor Preço</option>\r\n                </select>\r\n            </pwa-manual-filter>\r\n\t\t</div>\r\n\t</section>\r\n\r\n    <!-- Sessão dos produtos encontrados -->\r\n    <div class=\"pwat-results pwat-container\">\r\n\r\n        <!-- Botão de filtrar somente para o mobile -->\r\n        <a class=\"pwat-filters__funnel ismobile\" pwa-hook.click=\"category-filters-toggle-mobile\"><span class=\"lnr lnr-funnel\"></span></a>\r\n        <div class=\"pwat-filters__background-close\" pwa-hook.click=\"category-filters-toggle-mobile\"></div>\r\n\r\n        <!-- FILTROS -->\r\n\t\t<div class=\"pwat-filters\">\r\n            <a class=\"pwat-filters__close ismobile\" pwa-hook.click=\"category-filters-toggle-mobile\"><span class=\"lnr lnr-cross\"></span></a>\r\n\r\n            <pwa-tags-filter>\r\n                <!-- FILTERS ACTIVE -->\r\n                <section class=\"pwat-filters__founds\">\r\n                    <div class=\"pwat-filters__founds-header\">\r\n                        <h2 class=\"pwat-filters__founds-title\">Filtros Ativos</h2>\r\n                    </div>\r\n\r\n                    <div pwa-not-filters class=\"pwat-not__filters\">\r\n                        <p>Nenhum filtro ativo!</p>\r\n                    </div>\r\n\r\n                    <div pwa-has-filters class=\"pwat-filter__found\">\r\n                        <div pwa-each-filter class=\"pwat-filters__founds-item\">\r\n                            <div class=\"pwat-filters__active-item\">\r\n                                <p class=\"pwat-filters__active-item-label\">{{ filter.value }}</p>\r\n                                <a pwa-remove-filter=\"click\"><span class=\"lnr lnr-cross\"></span></a>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <p pwa-has-filters pwa-clean-filters class=\"pwat-clean-filters pwat-text-center\">Limpar todos os filtros</p>\r\n                </section>\r\n            </pwa-tags-filter>\r\n\r\n            <!-- \r\n                FILTRO CATEGORIAS\r\n                parâmetros: \r\n                    - pwa-parent: Através do id da categoria listada na página, são buscados todos os seus filhos/subcategorias.\r\n                    - size: limite máximo de subcategorias a serem listados\r\n            -->\r\n            <pwa-attribute-filter attribute=\"categories.code\" class=\"pwat-filters__founds pwat-filters__founds-anchor pwat-filters__categories\">\r\n                <a class=\"pwat-filters__founds-header\" pwa-hook.click=\"show-and-hide-founds-filter\">\r\n                    <h2 class=\"pwat-filters__founds-title\">Categorias</h2>\r\n                    <span pwa-has-filters>+</span>\r\n                </a>\r\n\r\n                <small pwa-not-filters class=\"pwat-filters__not-founds-header\"> \r\n                    Não encontramos subcategorias para estes produtos.\r\n                </small>\r\n\r\n                <div pwa-has-filters class=\"pwat-filters__founds-container\">\r\n                    <label pwa-each-filter class=\"pwat-filters__founds-wrapper\">\r\n                        <span pwa-filter-input=\"data-checked\" class=\"pwat-filters__founds-item\">\r\n                            {{ filter.name }} [{{ filter.count }}]\r\n                        </span>\r\n                    </label>\r\n                </div>\r\n            </pwa-attribute-filter>\r\n\r\n            <!-- \r\n                FILTRO CORES\r\n                parâmetros: \r\n                    - color:   é passado o nome do atributo no qual deseja buscar do elastic para que seja filtrado. No exemplo abaixo\r\n                                foi passado o atributo \"linha\" que se encontra em \"specifications.linha\", por padrão, a PWA já reconhece que os atributos dentro de\r\n                                specifications são filtráveis, porém se o atributo linha estiver fora de specifications e estiver na raiz onde o mesmo não possui nenhum \r\n                                atributo pai,  PWA também vai reconhecer normalmente. Qualquer outro atributo fora de specifications, é necessário passar o seu \"pai\"\r\n                                como exemplo: \"pai.atributo\".\r\n\r\n                    - size: limite máximo de subcategorias a serem listados\r\n            -->\r\n            <pwa-variation-filter code=\"color\" multiple class=\"pwat-filters__founds pwat-filters__founds-anchor pwat-filters__color\">\r\n                <a class=\"pwat-filters__founds-header\" pwa-hook.click=\"show-and-hide-founds-filter\">\r\n                    <h2 class=\"pwat-filters__founds-title\">Cores</h2>\r\n                    <span pwa-has-filters>+</span>\r\n                </a>\r\n\r\n                <small pwa-not-filters class=\"pwat-filters__not-founds-header\"> \r\n                    Esses produtos não possuem opções de cores.\r\n                </small>\r\n\r\n                <div pwa-has-filters class=\"pwat-filters__founds-container\">\r\n                    <label pwa-each-filter class=\"pwat-filters__founds-wrapper\">\r\n                        <span pwa-filter-input=\"data-checked\" class=\"pwat-filters__founds-item\">\r\n                            <span class=\"\" :style=\"{ backgroundColor: (filter || {}).name }\"></span>[{{ filter.count }}]\r\n                        </span>\r\n                    </label>\r\n                </div>\r\n            </pwa-variation-filter>\r\n\r\n            <!-- FILTRO TAMANHOS  -->\r\n            <pwa-variation-filter code=\"size\" multiple class=\"pwat-filters__founds pwat-filters__founds-anchor pwat-filters__size\">\r\n                <a class=\"pwat-filters__founds-header\" pwa-hook.click=\"show-and-hide-founds-filter\">\r\n                    <h2 class=\"pwat-filters__founds-title\">Tamanhos</h2>\r\n                    <span pwa-has-filters>+</span>\r\n                </a>\r\n\r\n                <small pwa-not-filters class=\"pwat-filters__not-founds-header\"> \r\n                    Produtos de tamanho único.\r\n                </small>\r\n\r\n                <div pwa-has-filters class=\"pwat-filters__founds-container\">\r\n                    <label pwa-each-filter class=\"pwat-filters__founds-wrapper\">\r\n                        <span pwa-filter-input=\"data-checked\" class=\"pwat-filters__founds-item\">\r\n                            {{ filter.label }} [{{ filter.count }}]\r\n                        </span>\r\n                    </label>\r\n                </div>\r\n            </pwa-variation-filter>\r\n\r\n            <!-- FILTRO POR RANGE DE PREÇO  -->\r\n            <pwa-price-filter max-size=\"5\" class=\"pwat-filters__founds pwat-filters__founds-anchor pwat-filters__price\">\r\n                <a class=\"pwat-filters__founds-header\" pwa-hook.click=\"show-and-hide-founds-filter\">\r\n                    <h2 class=\"pwat-filters__founds-title\">Range de Preço</h2>\r\n                    <span>+</span>\r\n                </a>\r\n\r\n                <div  class=\"pwat-filters__founds-container\">\r\n                    <div class=\"pwat-filters__founds-container-form\">\r\n                        <input  pwa-range-min-input  type=\"number\" name=\"price-from\"  placeholder=\"min\" class=\"efs-filtro-option-input\" >\r\n                        <small>até</small>\r\n                        <input pwa-range-max-input  type=\"number\" name=\"price-to\"  placeholder=\"max\" class=\"efs-filtro-option-input\" >\r\n                        <button pwa-range-submit type=\"button\"> Filtrar </button>\r\n                    </div>\r\n                </div>\r\n            </pwa-price-filter>\r\n\t\t</div>\r\n\r\n        <!-- Resultados e listagem dos produtos -->\r\n\t\t<div class=\"pwat-results__items\">\r\n\t\t\t<span class=\"pwat-results__quantity\">{{ $$paginate.count }} resultados</span>\r\n\r\n            <!-- \r\n                pwa-result-list traz a lista dos produtos encontrados\r\n                params = [\r\n                    9 => 'Quantidade de produtos listado por página',\r\n                    2 => 'Scroll para de carregar automáticamente ná página 2, da página três em diante, é necessário que o cliente clique em \"carregar mais..\" '\r\n                ]\r\n             -->\r\n\t\t\t<pwa-result-list size=\"24\" automatic-scrolls=\"0\" class=\"pwat-results__products\">\r\n\t\t\t\t<span pwa-not-result style=\"padding: 100px 0px; text-align: center; width: 100%\">Sem resultados para mostrar</span>\r\n\r\n                <div class=\"pwat-results__list\">\r\n\t\t\t\t\t<div pwa-each-product class=\"pwat-results__item\">\r\n\t\t\t\t\t\t<pwa-product-card>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\r\n                <!-- Loading em execução até que se encontre os produtos -->\r\n                <div pwa-is-loading class=\"pwat-results__list\">\r\n                    <pwa-skeleton w=\"33%\" h=\"500px\" size=\"24\">\r\n                        <pwa-skeleton-block h=\"350px\"/>\r\n                        <pwa-skeleton-block w=\"40%\" h=\"30px\"/>\r\n                        <pwa-skeleton-block h=\"30px\"/>\r\n                    </pwa-skeleton>\r\n                </div>\r\n                \r\n\t\t\t\t<div class=\"pwat-results_totop\">\r\n\t\t\t\t\t<a href=\"#\"><span class=\"lnr lnr-chevron-up\"></span></a>\r\n\t\t\t\t</div>\r\n\r\n                <!-- Botão carregar mais produtos.. -->\r\n\t\t\t\t<div pwa-not-last-page class=\"pwat-results__loading-more\">\r\n\t\t\t\t\t<a pwa-not-loading pwa-load-more><i class=\"lnr lnr-chevron-down\"></i>\r\n\t\t\t\t\t\tCarregar mais</a>\r\n\t\t\t\t</div>\r\n\t\t\t</pwa-result-list>\r\n\t\t</div>\r\n  </div>\r\n</main>"}]},"s3":true,"testcase":true,"shared":true,"indexed":false}