{"project":{"codes":[{"metadata":"hook","name":"__hook_cart","extension":"javascript","content":"{\n  \"cart-on-toggle\": component => {\n    const cart = document.getElementById(\"pwat-cart\");\n    const darken = document.getElementById(\"pwat-cart__darken\");\n\n    if (cart) {\n      if (cart.classList.contains(\"pwat-cart--open\")) {\n                darken.style.display = \"none\";\n        component.$scroll.unlock();\n      } else {\n        darken.style.display = \"block\";\n        component.$scroll.lock();\n      }\n\n      cart.classList.toggle(\"pwat-cart--open\");\n    }\n  },\n}"},{"metadata":"hook","name":"__hook_category","extension":"javascript","content":"{\r\n    \"--show-and-hide-founds-filter\": (component, el) => {\r\n        el.parentElement.classList.toggle('pwat-filters__founds-anchor--active');\r\n    },\r\n    \"--category-filters-toggle-mobile\": component => {\r\n        var filters = document.querySelector('.pwat-filters');\r\n        var backgroundClose = document.querySelector('.pwat-filters__background-close');\r\n\r\n        if (filters && backgroundClose) {\r\n            filters.classList.toggle('pwat-filters--active');\r\n            backgroundClose.classList.toggle('pwat-filters__background-close--active');\r\n        }\r\n    }\r\n}"},{"metadata":"hook","name":"__hooks","extension":"javascript","content":"{\r\n\r\n\t\r\n}"},{"metadata":"hook","name":"__hook_footer","extension":"javascript","content":"{\r\n    \"--footer-open-items\": (component, icon, elem) => {\r\n\t\tvar item = document.getElementById(elem);\r\n\r\n\t\tif (!item.classList.contains('pwat-footer__info--showed')) {\r\n\t\t\titem.classList.add('pwat-footer__info--showed');\r\n\t\t\ticon.children[0].classList.add('pwat-footer__info-icon-chevron--active');\r\n\t\t} else {\r\n\t\t\titem.classList.remove('pwat-footer__info--showed');\r\n\t\t\ticon.children[0].classList.remove('pwat-footer__info-icon-chevron--active');\r\n\t\t}\r\n\t},\r\n}"},{"metadata":"hook","name":"__hook_header","extension":"javascript","content":"{\r\n    \"--header-open-submenu\": (component, el) => {\r\n        el.event.preventDefault();\r\n        el.event.stopPropagation();\r\n\r\n        component.$setGlobalState('hasItems', true);\r\n\r\n\t\tvar sub = el.parentElement.children[3];\r\n        let subitems = el.parentElement.children[3].children[1];\r\n\r\n        setTimeout(() => {\r\n            if ((subitems || {}).children.length > 0) {\r\n                if (sub) {\r\n                    sub.classList.add('pwat-header__submenu--showed');\r\n                }\r\n            }\r\n        }, 300);\r\n    },\r\n\r\n    \"--header-close-submenu\": (component, el) => {\r\n        el.event.preventDefault();\r\n        el.event.stopPropagation();\r\n\r\n        let sub = el;\r\n\r\n        if (el.classList.contains('pwat-header__category-main')) {\r\n            if (window.innerWidth < 1024) {\r\n                component.$hook('--header-open-menu-mobile', component, {});\r\n            } else {\r\n                el = el.parentElement;\r\n                sub = el;\r\n            }\r\n        }\r\n\r\n        if (window.innerWidth > 1024 && sub.classList.contains('pwat-header__category-li')) {\r\n            sub = el.children[3];\r\n        }\r\n\r\n        if (!sub.classList.contains('pwat-header__submenu') && !el.classList.contains('pwat-header__submenu')) {\r\n            sub = el.closest('.pwat-header__submenu')\r\n        }\r\n\r\n        if (window.innerWidth < 1024) {\r\n            if (el.classList.contains('pwat-header__submenu-item'))  {\r\n                component.$hook('--header-open-menu-mobile', component, {})\r\n            }\r\n        }\r\n\r\n        if (sub) {\r\n            component.$setGlobalState('hasItems', false);\r\n            sub.classList.remove('pwat-header__submenu--showed');\r\n        }\r\n    },\r\n\r\n    \"--header-close-search-key-press\": (component, el) => {\r\n        el.addEventListener('keydown', function(e) {\r\n            if (e.key == \"Escape\" || e.key == \"Enter\") {\r\n                component.$hook('--header-open-search', component, {});\r\n            }\r\n        });\r\n    },\r\n\r\n    \"--header-open-search\": component => {\r\n        if (window.innerWidth < 1024) {\r\n            let menu = document.querySelector('.pwat-header__category');\r\n            if (menu) {\r\n                if (menu.classList.contains('pwat-header__category--showed')) {\r\n                    component.$hook('--header-open-menu-mobile', component, {});\r\n                }\r\n            }\r\n        }\r\n\r\n\t\tlet search = document.getElementById('pwat-header__search-input');\r\n        let closeSearch = document.querySelector('.pwat-header__search-input-close');\r\n\r\n        if (!search.classList.contains('pwat-header__search-input--showed')) {\r\n            search.classList.add('pwat-header__search-input--showed');\r\n        } else {\r\n            search.classList.remove('pwat-header__search-input--showed');\r\n        }\r\n\r\n        if (!closeSearch.classList.contains('pwat-header__search-input-close--active')) {\r\n            closeSearch.classList.add('pwat-header__search-input-close--active');\r\n        } else {\r\n            closeSearch.classList.remove('pwat-header__search-input-close--active');\r\n        }\r\n\r\n        setTimeout(()=> {\r\n            let input = document.querySelector('.pwat-header__search-input input');\r\n            if (input && search.classList.contains('pwat-header__search-input--showed')) {\r\n                input.focus();\r\n            }\r\n        }, 500);\r\n\t},\r\n\r\n    \"--header-open-menu-mobile\": (component) => {\r\n\t\tlet category = document.getElementById('pwat-header__category');\r\n        let hamb = document.querySelector('.pwat-hamburger');\r\n\r\n        if (category.classList.contains('pwat-header__category--showed')) {\r\n\t\t\tcategory.classList.remove('pwat-header__category--showed');\r\n\t\t\thamb.classList.remove('pwat-hamburger__bread--active');\r\n            component.$scroll.unlock();\r\n\t\t} else {\r\n\t\t\tcategory.classList.add('pwat-header__category--showed');\r\n\t\t\thamb.classList.add('pwat-hamburger__bread--active');\r\n            component.$scroll.lock();\r\n\t\t}\r\n\t}\r\n}"},{"metadata":"hook","name":"__hook_home","extension":"javascript","content":"{\r\n    \"--home-main-banner\": component => {\r\n        component.$slide('#pwat-main__banner-slide', {\r\n            slidesPerView:  1,\r\n            loop: true,\r\n            grabCursor: true,\r\n            navigation: {\r\n                nextEl: '.pwat-main__banner-slide__next',\r\n                prevEl: '.pwat-main__banner-slide__prev',\r\n            }\r\n        });\r\n    },\r\n\r\n    \"product-list-pwat-highlight__products-slide-loaded\": component => {\r\n        component.$slide('#pwat-highlight__slide', {\r\n            slidesPerView: window.innerWidth < 1024 ? 1 : 'auto',\r\n            // slidesPerColumn: window.innerWidth < 1024 ? 1 : 2,\r\n            spaceBetween: window.innerWidth < 1024 ? 0 : 10,\r\n            grabCursor: true,\r\n            navigation: {\r\n                nextEl: '.pwat-highlight__next',\r\n                prevEl: '.pwat-highlight__prev',\r\n            }\r\n        });\r\n    },\r\n\r\n    \"product-list-pwat-releases__products-slide-loaded\": component => {\r\n        component.$slide('#pwat-releases__slide', {\r\n            slidesPerView: window.innerWidth < 1024 ? 1 : 4,\r\n                spaceBetween: window.innerWidth < 1024 ? 0 : 10,\r\n                slidesPerGroup: window.innerWidth < 1024 ? 1 : 4,\r\n                grabCursor: true,\r\n                navigation: {\r\n                    nextEl: '.pwat-releases__slide__next',\r\n                    prevEl: '.pwat-releases__slide__prev',\r\n                },\r\n                pagination: {\r\n                    el: '.pwat-releases__pagination',\r\n                    dynamicBullets: window.innerWidth < 1024 ? true : false,\r\n                },\r\n        });\r\n    }\r\n}"},{"metadata":"hook","name":"__hook_local","extension":"javascript","content":"{\n    //local   \n}"},{"metadata":"hook","name":"__hook_pagseguro","extension":"javascript","content":"{\r\n    '+dsc-on-payment-changed': (component, payment) => {\r\n      const oldButton = document.getElementById('dsc-verify-next');\r\n      let customButton = document.getElementById('custom-finish-payment');\r\n\r\n      if (customButton === null) {\r\n        const footer = document.querySelector('.dsc-footer__next');\r\n        const newBtn = document.createElement('button');\r\n        newBtn.innerText = 'Finalizar compra';\r\n        newBtn.id = 'custom-finish-payment';\r\n        newBtn.style.display = 'none';\r\n        newBtn.classList.add('dsc-footer__next-button');\r\n        newBtn.classList.add('dsc-style__primary--background');\r\n\r\n        newBtn.onclick = () => {\r\n          component.loading = true;\r\n          component.finishing = true;\r\n          PagSeguroDirectPayment.onSenderHashReady(function(response) {\r\n            if (response.status == 'error') {\r\n              component.$toast(response.message).error;\r\n              component.loading = false;\r\n              component.finishing = false;\r\n\r\n              setTimeout(() => {\r\n                console.log('reseta btn1');\r\n                component.$hook(\r\n                  '+dsc-on-payment-changed',\r\n                  component,\r\n                  () => {},\r\n                  component.selected\r\n                );\r\n              }, 300);\r\n              return false;\r\n            }\r\n\r\n            component.$setGlobalState('pagseguroSender', response.senderHash);\r\n            console.log('response.senderHash', response.senderHash);\r\n\r\n            const paymentAction = async function(response) {\r\n              if ((response || {}).error) {\r\n                component.$toast(Object.values(response.errors)[0], {}, 0)\r\n                  .error;\r\n                component.loading = false;\r\n                component.finishing = false;\r\n\r\n                setTimeout(() => {\r\n                  console.log('reseta btn1');\r\n                  component.$hook(\r\n                    '+dsc-on-payment-changed',\r\n                    component,\r\n                    () => {},\r\n                    component.selected\r\n                  );\r\n                }, 300);\r\n                return;\r\n              }\r\n\r\n              try {\r\n                const { addressID } = component.$route.query;\r\n                const addresses = await component.$bridge(\r\n                  'customer/addresses',\r\n                  component.$$user\r\n                );\r\n                const address = addresses.filter(\r\n                  address => '' + address.id === '' + addressID\r\n                )[0];\r\n\r\n                const execute = await component.$http().post(\r\n                  'https://cors.onestic.com.br/https://middleware.pwa4all.com/payment/pagseguro/payment/execute',\r\n                  {\r\n                    quote_id: component.$$cart.id,\r\n                    data: {\r\n                      sender_hash: component.$$globalState.pagseguroSender,\r\n                      cart_token: response ? response.card.token : null,\r\n                    },\r\n                    items: component.$$cart.items.map(item => ({\r\n                      name: item.name,\r\n                      quantity: item.quantity,\r\n                      price: item.price,\r\n                    })),\r\n                    customer: {\r\n                      firstname: component.$$user.firstname,\r\n                      lastname: component.$$user.lastname,\r\n                      email: component.$$user.email,\r\n                      document: component.$$user.cpf,\r\n                    },\r\n                    address: {\r\n                      street: address.address,\r\n                      postcode: address.cep,\r\n                      neighborhood: address.neighborhood,\r\n                      city: address.city,\r\n                      state: address.state,\r\n                      uf: address.state,\r\n                      phone: address.phone || address.telephone,\r\n                      country: 'BR',\r\n                      number: address.number,\r\n                    },\r\n                    payment: {\r\n                      method: component.selected.type,\r\n                      ccbrand: response ? component.card.flag : null,\r\n                      installments: response ? component.card.installment : 1,\r\n                      total: +component.$$cart.total,\r\n                      amount: +component.$$cart.total,\r\n                      shipping: component.$$cart.delivery_price,\r\n                    },\r\n                  },\r\n                  {\r\n                    headers: {\r\n                      ecommerce: component.$pwa._id,\r\n                    },\r\n                  }\r\n                );\r\n\r\n                const { $bridge, $$cart, $$user, selected, card } = component;\r\n                const payment = {\r\n                  ...selected,\r\n                  data: card,\r\n                };\r\n                execute.data.method = 'pwapagseguro';\r\n\r\n                const order = await $bridge(\r\n                  'checkout/payment',\r\n                  $$cart,\r\n                  payment,\r\n                  $$user,\r\n                  null,\r\n                  false,\r\n                  execute.data\r\n                );\r\n\r\n                await component.$hook(\r\n                  '-dsc-payment-finished',\r\n                  component,\r\n                  () => {},\r\n                  order\r\n                );\r\n\r\n                component.$dataLayer('isSuccess').emit({\r\n                  pageName: 'transaction',\r\n                  customerDOB: null,\r\n                  customerZip: component.$$cart.addresses\r\n                    ? component.$$cart.addresses.shipping.postcode\r\n                    : '',\r\n                  clientEmail: component.$$user.email,\r\n                  deliveryTax: component.$$cart.delivery_price || '0',\r\n                  deliveryEstimate: '',\r\n                  totalSpent: component.$$cart.total,\r\n                  productsValue: component.$$cart.items\r\n                    .map(i => i.price)\r\n                    .join('|'),\r\n                  productsQuantity: component.$$cart.items\r\n                    .map(i => i.quantity)\r\n                    .join('|'),\r\n                  productsName: component.$$cart.items\r\n                    .map(i => i.name)\r\n                    .join('|'),\r\n                  productsSku: component.$$cart.items.map(i => i.sku).join('|'),\r\n                  productsEan: component.$$cart.items\r\n                    .map(i => i.codigo_barras || '')\r\n                    .join('|'),\r\n                  transactionId: order,\r\n                  transactionAffiliation: component.$pwa.name,\r\n                  transactionTotal: component.$$cart.subtotal,\r\n                  transactionShipping: component.$$cart.delivery_price || '0',\r\n                  orderAmount: component.$$cart.subtotal,\r\n                  orderAmount2: component.$$cart.subtotal * 100,\r\n                  customerId: component.$$user.id,\r\n                  transactionProductsIds: component.$$cart.items\r\n                    .map(i => i.id)\r\n                    .join('|'),\r\n                  transactionProducts: component.$$cart.items.map(item => ({\r\n                    productId: item.id,\r\n                    sku: item.id,\r\n                    quantity: item.quantity,\r\n                    productName: item.name,\r\n                    name: item.name,\r\n                    productPrice: item.price,\r\n                    price: item.payed || item.price,\r\n                    productDepartment: '',\r\n                    productCategory: item.categories_names || '',\r\n                    category: item.categories_names || '',\r\n                    productSubCategory: '',\r\n                    productBrand: item.brand || '',\r\n                  })),\r\n                });\r\n\r\n                component.$$setCart(null);\r\n                localStorage.removeItem('cart-id');\r\n\r\n                component.$router.push({\r\n                  name: 'dsc-success',\r\n                  query: {\r\n                    ...component.$route.query,\r\n                    orderID: order,\r\n                  },\r\n                });\r\n\r\n                component.loading = false;\r\n                component.finishing = false;\r\n              } catch (error) {\r\n                if (error.response) {\r\n                  component.$toast(error.response.data.message, {}, 0).error;\r\n                } else {\r\n                  component.$toast(error.message, {}, 0).error;\r\n                }\r\n                component.loading = false;\r\n                component.finishing = false;\r\n\r\n                setTimeout(() => {\r\n                  console.log('reseta btn1');\r\n                  component.$hook(\r\n                    '+dsc-on-payment-changed',\r\n                    component,\r\n                    () => {},\r\n                    component.selected\r\n                  );\r\n                }, 300);\r\n              }\r\n            };\r\n\r\n            if (component.selected.type === 'creditcard') {\r\n              PagSeguroDirectPayment.createCardToken({\r\n                cardNumber: component.card.number.replace(/ /g, ''),\r\n                brand: component.card.flag,\r\n                cvv: component.card.code,\r\n                expirationMonth: component.card.date.split('/')[0].trim(),\r\n                expirationYear: '20' + component.card.date.split('/')[1].trim(),\r\n                success: async function(response) {},\r\n                error: function(response) {\r\n                  component.$toast(response.message).error;\r\n                  component.loading = false;\r\n                  component.finishing = false;\r\n\r\n                  setTimeout(() => {\r\n                    console.log('reseta btn1');\r\n                    component.$hook(\r\n                      '+dsc-on-payment-changed',\r\n                      component,\r\n                      () => {},\r\n                      component.selected\r\n                    );\r\n                  }, 300);\r\n                },\r\n                complete: paymentAction,\r\n              });\r\n            } else {\r\n              paymentAction(null);\r\n            }\r\n          });\r\n        };\r\n\r\n        footer.appendChild(newBtn);\r\n      }\r\n\r\n      customButton = document.getElementById('custom-finish-payment');\r\n\r\n      if (payment.type === 'creditcard' || payment.type === 'billet') {\r\n        if (oldButton) {\r\n          oldButton.style.opacity = '0';\r\n          oldButton.style.zIndex = '-1';\r\n          oldButton.style.visibility = 'hidden';\r\n        }\r\n        if (customButton) {\r\n          customButton.style.display = 'flex';\r\n        }\r\n      } else {\r\n        if (oldButton) {\r\n          oldButton.style.opacity = '1';\r\n          oldButton.style.zIndex = '1';\r\n          oldButton.style.visibility = 'visible';\r\n        }\r\n\r\n        if (customButton) {\r\n          customButton.style.display = 'none';\r\n        }\r\n      }\r\n    },\r\n\r\n    'on-page-dsc-payment': async component => {\r\n        component.$toast('Lembrando que isso é um teste, seu pagamento não será válido.').warning\r\n\r\n        const alreadyScript = document.getElementById('pagseguro-script');\r\n        console.log('alreadyScript', alreadyScript);\r\n        if (alreadyScript === null) {\r\n            component.loading = true;\r\n            const script = document.createElement('script');\r\n            script.id = 'pagseguro-script';\r\n            script.type = 'text/javascript';\r\n            script.src =\r\n            'https://stc.sandbox.pagseguro.uol.com.br/pagseguro/api/v2/checkout/pagseguro.directpayment.js';\r\n            document.body.appendChild(script);\r\n\r\n            const post = await component.$http().post(\r\n            'https://cors.onestic.com.br/https://middleware.pwa4all.com/payment/pagseguro/payment/before-create-payment',\r\n            {\r\n                quote_id: component.$$cart.id,\r\n                items: component.$$cart.items.map(item => ({\r\n                name: item.name,\r\n                quantity: item.quantity,\r\n                price: item.price,\r\n                })),\r\n                customer: {\r\n                firstname: component.$$user.firstname,\r\n                lastname: component.$$user.lastname,\r\n                email: component.$$user.email,\r\n                document: component.$$user.cpf,\r\n                },\r\n                payment: {\r\n                method: 'creditcard',\r\n                total: +component.$$cart.total,\r\n                amount: +component.$$cart.total,\r\n                shipping: component.$$cart.delivery_price,\r\n                },\r\n            },\r\n            {\r\n                headers: {\r\n                ecommerce: component.$pwa._id,\r\n                },\r\n            }\r\n            );\r\n\r\n            component.$setGlobalState('pagseguroSession', post.data.response[0]);\r\n\r\n            console.log('session', post.data.response[0]);\r\n            PagSeguroDirectPayment.setSessionId(post.data.response[0]);\r\n\r\n            component.loading = false;\r\n      }\r\n    },\r\n}"},{"metadata":"hook","name":"__hook_product-page","extension":"javascript","content":"{\r\n    \"--zoomout\": component => {\r\n        if ($('.zoomContainer')) {\r\n            $('.zoomContainer').each((index, element) => {\r\n                element.remove();\r\n            })\r\n        }\r\n    },\r\n\r\n    \"--zoomin\": (component) => {\r\n        if (window.innerWidth > 1024) {\r\n            component.$hook('--zoomout', {}, () => { });\r\n                \r\n            $('.pwat-gallery__top .swiper-slide-active img').elevateZoom({\r\n                zoomType: \"inner\",\r\n                cursor: \"crosshair\",\r\n                zoomWindowFadeIn: 500,\r\n                zoomWindowFadeOut: 750\r\n            });\r\n        }\r\n    },\r\n\r\n    \"--swiper-gallery-product\": component=> {\r\n        var galleryThumbs = new Swiper('#pwat-gallery__list-images', {\r\n            spaceBetween: 10,\r\n            slidesPerView: 4,\r\n            freeMode: true,\r\n            watchSlidesVisibility: true,\r\n            watchSlidesProgress: true,\r\n        });\r\n\r\n        setTimeout(() => {\r\n            var galleryTop = new Swiper('#pwat-gallery__top', {\r\n                thumbs: {\r\n                    swiper: galleryThumbs\r\n                }\r\n            });\r\n        }, 500);\r\n    },\r\n\r\n    \"product-linkeds-relateds-loaded\": component => {\r\n        component.$slide('#pwat-product__linkeds-relateds-container', {\r\n            slidesPerView: window.innerWidth < 1024 ? 1 : 4,\r\n            spaceBetween: window.innerWidth < 1024 ? 0 : 10,\r\n            grabCursor: true,\r\n\r\n            pagination: {\r\n                el: '.pwat-product__linkeds-relateds-pagination',\r\n                dynamicBullets: true\r\n            },\r\n        });\r\n    }\r\n}"},{"metadata":"css","name":"__css_about-us","extension":"css","content":"/**\r\n* class .pwat-page__about-us-*\r\n*/\r\n.pwat-page__about-us {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding:  20px 10px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.pwat-page__about-us h1 {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.pwat-page__about-us img {\r\n  max-width: 100%;\r\n  width: 50%;\r\n}\r\n\r\n.pwat-video__about-us {\r\n    width: 100%;\r\n    height: 100%;\r\n}\r\n\r\n.pwat-video__about-us iframe {\r\n    width: 100%;\r\n    height: 660px;\r\n}\r\n.pwat-video__about-us {\r\n    width: 100%;\r\n    height: 100%;\r\n}\r\n\r\n.pwat-video__about-us iframe {\r\n    width: 100%;\r\n    height: 660px;\r\n}\r\n\r\n.pwat-page__about-us p {\r\n    line-height: 30px;\r\n}\r\n\r\n.pwat-page__about-us img {\r\n    margin: 20px 0;\r\n}\r\n\r\n@media screen and (max-width: 1024px) {\r\n    .pwat-page__about-us {\r\n        margin-top: 60px;\r\n    }\r\n\r\n    .pwat-video__about-us iframe {\r\n        height: 200px;\r\n    }\r\n}"},{"metadata":"css","name":"__async","extension":"css","content":"/\\*conteúdo aqui\\*/"},{"metadata":"css","name":"__css_cart","extension":"css","content":"/*\r\n    class .pwa-cart\r\n*/\r\n.pwat-cart {\r\n    width: 0;\r\n    visibility: hidden;\r\n    opacity: 0;\r\n    top: 0;\r\n    z-index: -1;\r\n    transition: all .25s ease-in;\r\n    position: fixed;\r\n    display: flex;\r\n    flex-flow: column;\r\n    background: #fff;\r\n    height: 100%;\r\n    right: 0;\r\n    padding: 0 0;\r\n    text-transform: none !important;\r\n    font-weight: 400 !important\r\n}\r\n.pwat-cart__darken {\r\n    width: 100%;\r\n    background: #000000a6;\r\n    height: 100%;\r\n    z-index: 998;\r\n    position: fixed;\r\n    display: none;\r\n    top: 0;\r\n    left: 0;\r\n}\r\n.pwat-cart--open {\r\n    transition: all .25s ease-in;\r\n    width: 400px;\r\n    visibility: visible;\r\n    opacity: 1;\r\n    z-index: 999\r\n}\r\n.pwat-cart strong:hover {\r\n    text-decoration: none !important\r\n}\r\n\r\n\r\n/*\r\n    class .pwa-cart__header\r\n*/\r\n.pwat-cart__header a .icon__close {\r\n    width: 10px;\r\n    height: 10px\r\n}\r\n\r\n.pwat-cart__header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    flex-direction: row;\r\n    padding: 10px 15px;\r\n}\r\n\r\n.pwat-cart__header div {\r\n    display: flex;\r\n    flex-direction: row;\r\n    align-items: center;\r\n    justify-content: center;\r\n    width: 100%\r\n}\r\n\r\n.pwat-cart__header div h2 {\r\n    font-size: 23px;\r\n    letter-spacing: 0px;\r\n    color: #01151e;\r\n    font-weight: 600;\r\n}\r\n\r\n.pwat-cart__header div span {\r\n    font-size: 19px;\r\n    text-align: center;\r\n    font-weight: 600;\r\n    color: #01151e;\r\n    margin-left: 5px;\r\n}\r\n\r\n.pwat-cart__header a .fa-times {\r\n    font-size: 20px;\r\n}\r\n\r\n.pwat-cart__header a .fa-times path {\r\n    fill: #43444A !important;\r\n}\r\n\r\n.pwat-cart__header>* {\r\n    align-self: center\r\n}\r\n\r\n.pwat-cart__header h2 {\r\n    cursor: default;\r\n    user-select: none\r\n}\r\n\r\n.pwat-cart__header a {\r\n    cursor: pointer\r\n}\r\n\r\n/*\r\n    class .pwat-cart__free-*\r\n*/\r\n.pwat-cart__free-shipping-warning {\r\n    position: relative;\r\n    width: 100%;\r\n    background: #01151e;\r\n}\r\n.pwat-cart__free-shipping-warning span {\r\n    color: #fff;\r\n    font-size: 13px;\r\n    line-height: 25px;\r\n}\r\n\r\n\r\n/*\r\n    class .pwat-cart__list-*\r\n*/\r\n.pwat-cart__list {\r\n    display: flex;\r\n    flex-flow: column;\r\n    list-style: none;\r\n    height: 100%;\r\n    overflow-y: auto;\r\n}\r\n.pwat-cart__list .pwat-cart__item {\r\n    display: flex;\r\n    width: calc(100% - 30px);\r\n    margin: 15px 15px;\r\n    flex-direction: row;\r\n    align-items: flex-start;\r\n    justify-content: space-between;\r\n    padding-bottom: 10px;\r\n    min-height: 125px;\r\n    border-bottom: 1px #f4f4f4 solid\r\n}\r\n.pwat-cart__list .pwat-cart__row-bag {\r\n    display: flex;\r\n    flex-flow: row;\r\n    justify-content: space-between;\r\n    align-items: flex-start;\r\n    width: 100%;\r\n}\r\n.pwat-cart__list .pwat-cart__options {\r\n    margin-top: 5px;\r\n}\r\n.pwat-cart__list .pwat-cart__options li {\r\n    list-style: none;\r\n    font-size: 13px;\r\n}\r\n.pwat-cart__list .pwat-input__number {\r\n    width: 95px !important;\r\n    display: flex;\r\n    justify-content: center !important;\r\n    align-items: center !important;\r\n    flex-flow: row;\r\n    border: 1px solid #dfdfdf;\r\n    padding: 5px;\r\n    border-radius: 5px;\r\n    margin-top: 15px;\r\n}\r\n.pwat-cart__list .pwat-input__number span {\r\n    border: 2px solid #dfdfdf;\r\n    border-radius: 50%;\r\n    width: 20px;\r\n    height: 20px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    margin: 0;\r\n    font-size: 14px;\r\n    font-weight: 600;\r\n    cursor: pointer;\r\n    margin-top: 0 !important;\r\n}\r\n.pwat-cart__list .pwat-input__number input {\r\n    width: 50%;\r\n    border: none;\r\n    -webkit-border-image: none;\r\n    text-align: center;\r\n    font-size: 17px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n.pwat-cart__list .pwat-cart__row-bag-container {\r\n    display: flex;\r\n    flex-flow: row;\r\n    justify-content: space-between;\r\n    align-items: flex-start;\r\n    width: 100%;\r\n}\r\n.pwat-cart__list .pwat-cart__row-bag-container img {\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n}\r\n.pwat-cart__list .pwat-cart__product-img {\r\n    height: 100%;\r\n    width: 105px;\r\n    border: 1px solid #dfdfdf;\r\n    border-radius: 5px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    padding: 5px;\r\n    margin-right: 15px;\r\n}\r\n.pwat-cart__list .pwat-cart__product-oldprice {\r\n    margin-right: 5px;\r\n    text-decoration: line-through;\r\n    color: #484848;\r\n}\r\n\r\n.pwat-cart__list .pwat-cart__row-pay {\r\n    width: 65%;\r\n    display: flex;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    flex-flow: column;\r\n}\r\n.pwat-cart__list .pwat-cart__row-pay h3 {\r\n    font-weight: 400;\r\n    font-size: 16px;\r\n    width: 95%;\r\n    line-height: 20px;\r\n}\r\n.pwat-cart__list .pwat-cart__row-pay span {\r\n    font-size: 13px;\r\n    color: #787878;\r\n    margin-top: 5px;\r\n}\r\n\r\n/*\r\n    Skeleton\r\n*/\r\n.pwat-cart__skeleton-col {\r\n    width: 50%;\r\n    display: flex;\r\n    flex-flow: column;\r\n    margin-left: 15px;\r\n}\r\n.pwat-cart__list .p4a-skeleton {\r\n    margin-left: 15px;\r\n    margin-top: 15px;\r\n    margin-bottom: 15px;\r\n}\r\n\r\n/*\r\n    class .pwat-cart__footer-*\r\n*/\r\n.pwat-cart__footer {\r\n    display: flex;\r\n    flex-flow: column;\r\n    width: 100%;\r\n    margin: 0 !important;\r\n    background: #f6f6f6;\r\n}\r\n.pwat-cart__footer .cart__subtotal {\r\n    display: flex;\r\n    flex-flow: row;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n}\r\n.pwat-cart__footer .pwat-cart__subtotal {\r\n    display: flex;\r\n    flex-flow: row;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    padding: 15px 15px;\r\n    border-top: 1px solid #dfdfdf;\r\n}\r\n.pwat-cart__footer .pwat-cart__subtotal h3 {\r\n    color: #43444A;\r\n    font-size: 14px;\r\n    font-weight: 900 !important;\r\n}\r\n.pwat-cart__footer .pwat-cart__subtotal span {\r\n    color: #43444A;\r\n    font-size: 14px;\r\n    font-weight: 900 !important;\r\n}\r\n.pwat-cart__footer .pwat-freight__result {\r\n    font-size: 13px;\r\n    margin-top: 5px;\r\n}\r\n\r\n.pwat-cart__footer .pwat-form__btn {\r\n    font-size: 13px;\r\n    background: #01151e;\r\n    color: #fff;\r\n    height: 45px;\r\n    width: 125px;\r\n    border-radius: 30px;\r\n    border: none;\r\n}\r\n.pwat-cart__footer .pwat-form__input-checked {\r\n    color: #fff;\r\n    background: green;\r\n    width: 20px;\r\n    height: 20px;\r\n    border-radius: 50%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    cursor: pointer;\r\n    position: absolute;\r\n    right: 5px;\r\n    top: 13px;\r\n}\r\n.pwat-cart__footer .pwat-cart__discout {\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    width: calc(100% - 30px);\r\n    padding: 15px 15px;\r\n    border-top: 1px solid #dfdfdf;\r\n}\r\n.pwat-cart__footer .pwat-cart__discout-title h3 {\r\n    font-weight: 300;\r\n    font-size: 15px;\r\n}\r\n.pwat-cart__footer .pwat-cart__discout .pwat-form__input {\r\n    width: 100%;\r\n    position: relative;\r\n    display: flex;\r\n    flex-flow: row;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n.pwat-cart__footer .pwat-cart__discout .pwat-form__input:last-child {\r\n    margin-top: 15px;\r\n}\r\n.pwat-cart__footer .pwat-cart__discout .pwat-form__input input {\r\n    width: calc(100% - 160px);\r\n    height: 45px;\r\n    border: 1px solid #dfdfdf;\r\n    padding: 0 15px;\r\n    border-radius: 30px;\r\n}\r\n.pwat-cart__footer .pwat-cart__discout--success {\r\n    display: flex;\r\n    flex-flow: row;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin-top: 15px;\r\n}\r\n.pwat-cart__footer .pwat-cart__discout--success h3 {\r\n    font-weight: 300;\r\n    font-size: 15px;\r\n}\r\n.pwat-cart__footer .pwat-cart__discout--success span {\r\n    color: #787878;\r\n}\r\n.pwat-cart__footer .pwat-cart__total {\r\n    display: flex;\r\n    flex-flow: row;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 15px 15px;\r\n    border-top: 1px solid #dfdfdf;\r\n}\r\n.pwat-cart__footer .pwat-cart__total > * {\r\n    font-weight: 500;\r\n    font-size: 23px;\r\n    color: #01151e;\r\n}\r\n.pwat-cart__footer .pwat-cart__btn {\r\n    margin-top: 15px;\r\n    margin-bottom: 15px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    width: calc(100% - 30px);\r\n    padding: 0 15px;\r\n}\r\n.pwat-cart__footer .pwat-cart__btn a {\r\n    background: #01151e;\r\n    color: #fff;\r\n    padding: 15px;\r\n    text-align: center;\r\n    font-size: 13px;\r\n    width: calc(100% - 30px);\r\n    letter-spacing: 2px;\r\n    border-radius: 30px;\r\n}\r\n\r\n\r\n/*\r\n    class .pwat-cart__empty\r\n*/\r\n.pwat-cart__empty {\r\n    position: absolute;\r\n    top: 50px;\r\n    display: flex;\r\n    flex-flow: column;\r\n    align-items: center;\r\n    justify-content: center;\r\n    width: 100%;\r\n    text-align: center;\r\n    height: 100%;\r\n    background: #fff;\r\n    z-index: 999;\r\n}\r\n.pwat-cart__empty .lnr-heart {\r\n    font-size: 40px !important;\r\n    margin-bottom: 15px;\r\n    color: #01151e;\r\n}\r\n.pwat-cart__empty a {\r\n    background: #01151e;\r\n    color: #fff;\r\n    position: relative;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    width: 130px;\r\n    height: 45px;\r\n    margin-top: 20px;\r\n    border-radius: 30px;\r\n    font-size: 13px;\r\n}\r\n.pwat-cart__empty a:hover {\r\n    transform: scale(1.05);\r\n    transition: all .25s ease;\r\n    cursor: pointer;\r\n    background: #01151e;\r\n}\r\n.pwat-cart__empty a span {\r\n    color: #fff;\r\n    position: absolute;\r\n    top: 10px;\r\n}\r\n\r\n\r\n/*\r\n    class .pwat-cart__delete-item\r\n*/\r\n.pwat-cart .pwat-cart__delete-item {\r\n    background: #dfdfdf;\r\n    width: 20px;\r\n    height: 20px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    border-radius: 50%;\r\n}\r\n.pwat-cart .pwat-cart__delete-item .lnr-trash {\r\n    font-size: 12px;\r\n    cursor: pointer;\r\n}\r\n\r\n\r\n\r\n/* class .pwat-cart__fill */\r\n.pwat-cart__fill-progress {\r\n    position: absolute;\r\n    height: 100%;\r\n    width: 0px;\r\n    background: linear-gradient(to right, rgba(255, 255, 255, .1), rgba(255, 255, 255, .12), rgba(255, 255, 255, .3));\r\n    transition: width 1s linear;\r\n    top: 0;\r\n    left: 0;\r\n}\r\n\r\n/* class .pwat-cart__quant-freight */\r\n.pwat-cart__quant-freight-free {\r\n    color: #fff;\r\n    margin: 13px;\r\n    font-size: 13px;\r\n}\r\n\r\n\r\n@media (max-width: 767px) {\r\n    /* \r\n        class .pwat-cart__*\r\n    */\r\n\r\n    .pwat-cart--open {\r\n        width: 95%;\r\n    }\r\n    .pwat-cart__header {\r\n        padding: 10px;\r\n    }\r\n    .pwat-cart__header div h2, .pwat-cart__header div span {\r\n        font-size: 17px;\r\n    }\r\n    .pwat-cart__free-shipping-warning {\r\n        padding: 10px;\r\n        width: calc(100% - 20px);\r\n    }\r\n    .pwat-cart__free-shipping-warning span {\r\n        font-size: 13px;\r\n        line-height: 18px;\r\n    }\r\n    .pwat-cart__quant-freight-free {\r\n        margin: 0;\r\n    }\r\n    .pwat-cart__product-img {\r\n        height: 90px;\r\n        width: 90px;\r\n    }\r\n    .pwat-cart__row-pay h3 {\r\n        font-size: 13px;\r\n        line-height: 18px;\r\n    }\r\n    .pwat-input__number {\r\n        margin-top: 10px;\r\n    }\r\n    .pwat-input__number input {\r\n        font-size: 12px;\r\n    }\r\n    .pwat-cart__total > * {\r\n        font-size: 18px;\r\n    }\r\n}"},{"metadata":"css","name":"__css_category-page","extension":"css","content":"/**\r\n* PÁGINA DE CATEGORIA/PESQUISA\r\n* class .pwat-filter\r\n*/\r\n.pwat-filter {\r\n  display: flex;\r\n  flex-flow: row;\r\n  justify-content: flex-end;\r\n  align-items: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.pwat-filter__submit {\r\n  display: flex;\r\n  flex-flow: row;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-size: 15px;\r\n  color: #2f2f2f;\r\n}\r\n\r\n.pwat-filter__submit h2 {\r\n    font-size: 15px;\r\n}\r\n\r\n.pwat-filter__select {\r\n  margin-left: 20px;\r\n  border: none;\r\n  border-radius: 20px;\r\n  padding: 5px;\r\n  background: #01151e;\r\n  width: 120px;\r\n  color: #fff;\r\n}\r\n\r\n.pwat-filters{\r\n    margin-right: 30px;\r\n    width: 198px;\r\n}\r\n\r\n.pwat-filters__founds {\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    width: 100%;\r\n    margin-bottom: 30px;\r\n}\r\n\r\n.pwat-filters__founds-header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    width: 100%;\r\n}\r\n\r\n.pwat-filters__founds-title {\r\n    font-weight: 400;\r\n    font-size: 17px;\r\n}\r\n\r\n.pwat-not__filters p {\r\n    font-size: 13px;\r\n    color: #7c7c7c;\r\n}\r\n\r\n.pwat-not__filters p {\r\n    font-size: 13px;\r\n    color: #7c7c7c;\r\n}\r\n\r\n.pwat-filters__founds-container {\r\n    display: flex;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    flex-flow: column;\r\n    width: calc(100% - 20px);\r\n    background: #f8f8f8;\r\n    padding: 10px;\r\n    margin-top: 5px;\r\n    border-radius: 10px;\r\n}\r\n\r\n.pwat-filters__founds-anchor .pwat-filters__founds-container {\r\n    overflow: hidden;\r\n    height: 0;\r\n    padding: 0;\r\n    margin: 0;\r\n    transition: all .3s;\r\n}\r\n\r\n.pwat-filters__founds-anchor--active .pwat-filters__founds-container {\r\n    overflow: unset !important;\r\n    height: auto !important;\r\n    padding: 10px !important;\r\n    margin-top: 5px !important;\r\n    transition: all .3s;\r\n}\r\n\r\n.pwat-filters__founds-anchor .pwat-filters__founds-header span {\r\n    transition: all .3s;\r\n}\r\n\r\n.pwat-filters__founds-anchor--active .pwat-filters__founds-header span {\r\n    transform: rotateZ(45deg);\r\n    transition: all .3s;\r\n}\r\n\r\n.pwat-filters__founds-wrapper:not(:last-child) {\r\n    margin-bottom: 7px;\r\n}\r\n\r\n.pwat-filters__not-founds-header {\r\n    font-size: 11px;\r\n    color: #b4b4b4;\r\n}\r\n\r\n.pwat-filters__founds-wrapper .pwat-filters__founds-item {\r\n    font-size: 14px;\r\n    color: #000;\r\n    font-weight: 500;\r\n    cursor: pointer;\r\n}\r\n\r\n.pwat-filters__color .pwat-filters__founds-item label span {\r\n    width: 10px;\r\n    height: 10px;\r\n}\r\n\r\n.pwat-filters__color .pwat-filters__founds-item span {\r\n    width: 35px !important;\r\n    height: 17px !important;\r\n    margin-right: 10px;\r\n    border: 1px solid #ededed;\r\n    border-radius: 4px